<apex:page docType="html-5.0" sidebar="false" showHeader="false" title="Get Protected" controller="ESalesApplicationCtrl">
    <head>
        <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, viewport-fit=cover"/>
    </head>

    <div class="bs col-xs-12 form-horizontal">
    <apex:form id="confirmationfrm">
        <apex:actionFunction action="{!removeChild}" name="removeChild" namespace="ServerESales"
                             reRender="wrapper,wrapperApp" oncomplete="Cigna.ESales.finishLoading();" status="loader-icon"
        />
        <apex:actionFunction action="{!removeSpouse}" name="removeSpouse" namespace="ServerESales"
                             reRender="wrapper,wrapperApp" oncomplete="Cigna.ESales.finishLoading();" status="loader-icon"
        />
        <apex:actionFunction action="{!removeRider}" name="removeRider" namespace="ServerESales"
                             reRender="wrapper,wrapperApp,confirmation-section" oncomplete="Cigna.ESales.finishLoading();" status="loader-icon"
        />
        <apex:actionFunction action="{!nextStep}" name="nextStep" namespace="ServerESales"
                         reRender="wrapper,wrapperApp,errmessage" oncomplete="refreshEsignature();retrieveAUWURL_JS();Cigna.ESales.finishLoading();" status="loader-icon"
        />
        <apex:actionFunction action="{!prevStep}" name="prevStep" namespace="ServerESales"
                             reRender="wrapper,wrapperApp" oncomplete="Cigna.ESales.finishLoading();" status="loader-icon"
        />
        <apex:actionFunction action="{!saveRecord}" name="saveRecord" namespace="ServerESales"
                             reRender="wrapper,wrapperApp" status="loader-icon"
        />

        <apex:outputPanel id="confirmation-section" rendered="{!currentStep == STEP_CONFIRMATION}" styleClass="bs row" layout="block">
            <script>
                Cigna.ESales.clearUWStatusPolling();
                $('div.header-bar').show();
                window.scrollTo(0, 0);
            </script>
            <apex:inputHidden id="remove-child-id" value="{!removeChildId}" />
            <apex:inputHidden id="remove-rider-code" value="{!removeRiderCode}" /> 
            <apex:inputHidden id="remove-rider-role" value="{!removeRiderRole}" />
            <!-- application & quote result -->
            <div class="bs col-xs-12 form-horizontal">
                <!-- Application_Result !DMB -->
                <apex:outputPanel rendered="{!txn.Basic_Plan_Code__c != 'DMB' && !EnableLoadingDisplay}">
                <div class="bs" style="padding-bottom: 20px;">
                    <p><h4 class="bs cigna-fg-blue-medium">{!$Label.Application_Result}</h4></p>
                    <apex:outputPanel rendered="{!isCustomerPortal}">
                        <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Accept' && txn.AUW_Overall_Result__c == 'Accept'}" layout="none">
                            <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_Exclusion}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_Decline}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note2}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Accept}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Accept' && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_refer_with_Exclusion}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline || hasAccept}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none">
                                <p>{!$Label.eSales_AUW_result_Note2}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>

                        <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Referred'}" layout="none">
                            <p>{!$Label.eSales_AUW_result_PH_refer}</p>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Decline'}" layout="none">
                            <p>{!$Label.eSales_AUW_result_PH_Decline}</p>
                        </apex:outputPanel>
                    </apex:outputPanel>

                    <apex:outputPanel rendered="{!!isCustomerPortal}">
                        <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Accept' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Accept'}" layout="none">
                            <!--apex:outputPanel rendered="{!hasExclusion}" layout="none"--><!--commentted by application result change SUQ Connor 11/08/2021-->
                            <apex:outputPanel rendered="{! hasExclusion && !hasLoading}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_with_Exclusion}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 start-->
                            <apex:outputPanel rendered="{! hasExclusion && hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_with_Exclusion_Loading}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! !hasExclusion && hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 end-->
                            <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_with_Decline}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Accept}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Accept' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                            <!--application result change SUQ Connor 11/08/2021 start-->
                            <apex:outputPanel rendered="{! hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 end-->
                            <br/>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Referred' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Referred'))}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                            <br/>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                        </apex:outputPanel>

                        <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Decline' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Decline'))}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Decline}</p>
                        </apex:outputPanel>
                    </apex:outputPanel>
                </div>
                </apex:outputPanel>

                <!-- Quotation_Result DMB and LPC product with loading-->
                <apex:outputPanel rendered="{!txn.Basic_Plan_Code__c == 'DMB' || EnableLoadingDisplay}">
                    <div class="bs" style="padding-bottom: 20px;">
                        <p><h4 class="bs cigna-fg-blue-medium">{!$Label.eSales_Quotation_Result}</h4></p>
                        <!-- Customer Portal -->
                        <apex:outputPanel rendered="{!isCustomerPortal}">
                            <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Accept'}" layout="none">

                                <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                    <p>{!IF(hasLoading,$Label.eSales_AUW_Result_with_Exclusion_Loading,$Label.eSales_AUW_result_with_Exclusion)}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                    <apex:outputPanel rendered="{!hasLoading && !hasExclusion}" layout="none">
                                        <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                                    </apex:outputPanel>

                                    <p>{!$Label.eSales_AUW_result_with_Decline}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_Accept}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!!hasDecline && !hasExclusion && hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!hasExclusion || hasDecline || hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_Note1}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_Note2}</p>
                                </apex:outputPanel>

                            </apex:outputPanel>

                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Accept' && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                                <apex:outputPanel rendered="{!hasExclusion && !hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_refer_with_Exclusion}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!hasExclusion && hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_with_refer_loading}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!!hasExclusion && hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_with_refer_loading_without_Exclusion}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!!hasExclusion && !hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_with_refer}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!hasExclusion || hasDecline || hasAccept}" layout="none">
                                    <p>{!$Label.eSales_AUW_result_Note1}</p>
                                </apex:outputPanel>
                                <apex:outputPanel layout="none">
                                    <p>{!$Label.eSales_AUW_result_Note2}</p>
                                </apex:outputPanel>
                            </apex:outputPanel>

                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Referred' || (txn.Number_of_Adult__c == 0 && txn.Cover_Child__c && txn.AUW_Overall_Result__c == 'Referred')}" layout="none">
                                <p>{!$Label.eSales_AUW_result_PH_refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Decline'}" layout="none">
                                <p>{!$Label.eSales_AUW_result_PH_Decline}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <!-- ! Customer Portal -->
                        <apex:outputPanel rendered="{!!isCustomerPortal}">
                            <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Accept'}" layout="none">
                                <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                    <p>{!IF(hasLoading,$Label.eSales_Brk_AUW_Result_with_Exclusion_Loading,$Label.eSales_Brk_AUW_Result_with_Exclusion)}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                    <apex:outputPanel rendered="{!hasLoading && !hasExclusion}" layout="none">
                                        <p>{!$Label.eSales_Brk_AUW_Result_Accept_Loading}</p>
                                    </apex:outputPanel>

                                    <p>{!$Label.eSales_Brk_AUW_Result_with_Decline}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none">
                                    <p>{!$Label.eSales_Brk_AUW_Result_Accept}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!!hasDecline && !hasExclusion && hasLoading}" layout="none">
                                    <p>{!$Label.eSales_Brk_AUW_Result_Accept_Loading}</p>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!hasExclusion || hasDecline || hasLoading}" layout="none">
                                    <p>{!$Label.eSales_Brk_AUW_Result_Note1}</p>
                                </apex:outputPanel>
                                <!--<apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                    <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                                </apex:outputPanel>-->
                                <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3 || hasExclusionPI || hasExclusionC1_Rider || hasExclusionC2_Rider || hasExclusionC3_Rider || hasExclusionPI_Rider}" layout="none">
                                    <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                                </apex:outputPanel>

                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Accept' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                                <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                                <!--dmb application result change SUQ Connor 2021/11/12 start-->
                                <apex:outputPanel rendered="{! hasLoading}" layout="none">
                                    <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                                </apex:outputPanel>
                                <!--dmb application result change SUQ Connor 2021/11/12 end-->
                                <br/>
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!(txn.AUW_Result_PH__c == 'Referred' || (!txn.PH_is_PI__c && txn.AUW_Result_PI__c == 'Referred') || (txn.Number_of_Adult__c == 0 && txn.Cover_Child__c && txn.AUW_Overall_Result__c == 'Referred'))}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                                <br/>
                                <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                            </apex:outputPanel>

                            <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Decline'}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Decline}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                    </div>
                </apex:outputPanel>
            </div>

            <apex:outputPanel rendered="{!txn.Basic_Plan_Code__c == 'DMB' || EnableLoadingDisplay}">
                <div class="bs col-xs-12 form-horizontal" style="padding-bottom: 20px;">
                    <p><h4 class="bs cigna-fg-blue-medium">{!$Label.eSales_Coverage_Details}</h4></p>

                    <div class="bs form-group">
                        <div class="bs col-sm-4 col-md-3">
                            <label>{!$Label.Selected_Benefit}</label>
                        </div>
                        <div class="bs col-sm-8 col-md-9">
                            {!product.Name__c}
                        </div>
                    </div>

                    <apex:outputPanel rendered="{!!txn.Different_Plan_Level__c}">
                        <apex:outputPanel rendered="{!!ISBLANK(txn.Plan_Level__c)}">
                            <div class="bs form-group">
                                <div class="bs col-sm-4 col-md-3">
                                    <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.Plan_Level__c.Label}" for="Confirm-DMBPlanLevel"/>
                                </div>
                                <div class="bs col-sm-8 col-md-9">
                                    <apex:outputField id="Confirm-DMBPlanLevel" value="{!txn.Plan_Level__c}" />
                                </div>
                            </div>
                        </apex:outputPanel>

                        <apex:outputPanel rendered="{!hasRiders}">
                            <div class="bs form-group">
                                <div class="bs col-sm-4 col-md-3">
                                    <apex:outputLabel value="{!$Label.eSales_Additional_Benefit}" for="Confirm-DMBRiders"/>
                                </div>
                                <div class="bs col-sm-8 col-md-9">
                                    <apex:repeat value="{!selectedRiders}" var="rider">
                                        <p>{!rider.Name__c}</p>
                                    </apex:repeat>
                                </div>
                            </div>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!hasRiders && !ISBLANK(txn.Rider_Level__c)}">
                            <div class="bs form-group">
                                <div class="bs col-sm-4 col-md-3">
                                    <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.Rider_Level__c.Label}" for="Confirm-DMBRiderPlanLevel"/>
                                </div>
                                <div class="bs col-sm-8 col-md-9">
                                    <apex:outputField id="Confirm-DMBRiderPlanLevel" value="{!txn.Rider_Level__c}" />
                                </div>
                            </div>
                        </apex:outputPanel>
                    </apex:outputPanel>
                </div>
            </apex:outputPanel>

            <!-- person information in !DMB -->
            <apex:outputPanel rendered="{!txn.Basic_Plan_Code__c != 'DMB' && !EnableLoadingDisplay}">
                <!-- ph info -->
                <div class="bs col-xs-12 form-horizontal">
                    <div class="bs" style="padding-bottom: 20px;">
                        <h4 class="bs cigna-fg-blue-medium">{!IF((txn.PH_is_PI__c || isCustomerPortal), $Label.Policy_Holder_s_Application, $Label.Policy_Holder_s_Information)}</h4>
                    </div>
                    <apex:outputPanel styleClass="bs form-group" layout="block" rendered="{!txn.PH_is_PI__c || isCustomerPortal}">
                        <div class="bs col-sm-3">
                            <apex:outputLabel value="{!$Label.Application_Result}" for="Confirm-Application-Result"/>
                        </div>
                        <div class="bs col-sm-9">
                            <!--ph application result change 09/08/2021 Connor SUQ start-->
                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Accept' && !hasExclusionPH && !hasLoadingPH}" layout="none">  
                                <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Accept' && hasExclusionPH && !hasLoadingPH}" layout="none">
                                <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept_With_Condition}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! isCustomerPortal && txn.AUW_Result_PH__c == 'Accept' && !hasExclusionPH && hasLoadingPH}" layout="none"> 
                                <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i>{!$Label.AUW_Overall_Refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! !isCustomerPortal && txn.AUW_Result_PH__c == 'Accept' && !hasExclusionPH && hasLoadingPH}" layout="none"> 
                                <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i>{!$Label.AUW_Overall_Accept_With_Loading}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! isCustomerPortal && txn.AUW_Result_PH__c == 'Accept' && hasExclusionPH && hasLoadingPH}" layout="none"> 
                                <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i> {!$Label.AUW_Overall_Refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! !isCustomerPortal && txn.AUW_Result_PH__c == 'Accept' && hasExclusionPH && hasLoadingPH}" layout="none"> 
                                <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept_With_2_Conditions}</p>
                            </apex:outputPanel>
                            <!--ph application result change 09/08/2021 Connor SUQ end-->
                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Referred'}" layout="none">
                                <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i> {!$Label.AUW_Overall_Refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!txn.AUW_Result_PH__c == 'Decline'}" layout="none">
                                <p><i class="bs cigna-fg-red-alert fa fa-times-circle fg-lg"></i> {!$Label.AUW_Overall_Decline}</p>
                            </apex:outputPanel>
                        </div>
                    </apex:outputPanel>
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.Policy_Holder_First_Name__c.Label}" for="Confirm-PH-Firstname"/>
                        </div>
                        <div class="bs col-sm-9">
                            <apex:outputText id="Confirm-PH-Firstname" value="{!txn.Policy_Holder_First_Name__c}"
                            />
                        </div>
                    </div>
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.Policy_Holder_Last_Name__c.Label}" for="Confirm-PH-Lastname"/>
                        </div>
                        <div class="bs col-sm-9">
                            <apex:outputText id="Confirm-PH-Lastname" value="{!txn.Policy_Holder_Last_Name__c}"
                            />
                        </div>
                    </div>
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <!--<apex:outputLabel value="{!IF(isCustomerPortal, $Label.HKID, $ObjectType.ESales_Transaction__c.Fields.HKID__c.Label)}" for="Confirm-PH-HKID"/>-->
                            <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.HKID__c.Label}" for="Confirm-PH-HKID"/>
                        </div>
                        <div class="bs col-sm-9">
                            <apex:outputText id="Confirm-PH-HKID" value="{!txn.HKID__c}"
                            />
                        </div>
                    </div>
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <apex:outputLabel value="{!$ObjectType.ESales_Transaction__c.Fields.Date_of_Birth_PH__c.Label}" for="Confirm-PH-DOB"/>
                        </div>
                        <div class="bs col-sm-9">
                            <apex:outputText id="Confirm-PH-DOB" value="{0, date, yyyy/MM/dd}">
                                <apex:param value="{!txn.Date_of_Birth_PH__c}" />
                            </apex:outputText>
                        </div>
                    </div>
                    <!--<apex:outputPanel rendered="{!hasExclusionPH && txn.AUW_Overall_Result__c != 'Referred'}" layout="none">-->
                    <!-- Ray:Show the exclusion -->
                    <apex:outputPanel rendered="{!hasExclusionPH && txn.AUW_Result_PH__c == 'Accept'}" layout="none">                               
                        <div class="bs form-group">
                            <div class="bs col-sm-3">
                                <apex:outputLabel value="{!$Label.eSales_Exclusion}" for="Confirm-Exclusion"/>
                            </div>
                            <div class="bs col-sm-9">
                                <apex:repeat value="{!exclusionPH}" var="ph">
