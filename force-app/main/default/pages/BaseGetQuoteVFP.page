            function isNumberKey(charCode, min, max) {
                if ((charCode >= 48 && charCode <= 57) || 
                    $.inArray(charCode, [8, 127]) !== -1) {
                    if (!isNaN(min) && !isNaN(max) && typeof min != undefined && typeof max != undefined) {
                        if ((charCode - 48) >= min && (charCode - 48) <= max) {
                            return true;
                        } else {
                            return false;
                        }
                    } else {
                        return true;
                    }
                } else {
                    return false;
                }
            }
            
