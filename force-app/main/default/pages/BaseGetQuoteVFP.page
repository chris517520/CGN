    <apex:outputPanel id="init-progress-bar">
        <script>
            function initProgressBar(){
                             
                             //var personRole = "{!selectedPersonRole}";                                      
                             var personRole = $("input[type='hidden'][id$='param-selected-person-role']").val();
                             var size = '';
                             var deductList = '';
                             if(personRole == 'a2' || personRole == 'A2'){
                                 personRole = '-a2';
                             } else if(personRole == 'a1' || personRole == 'A1'){
                                 personRole = '-a1';                             
                             } else if(personRole == 'c1' || personRole == 'C1'){
                                 personRole = '-c1';                             
                             } else if(personRole == 'c2' || personRole == 'C2'){
                                 personRole = '-c2';
                             } else if(personRole == 'c3' || personRole == 'C3'){
                                 personRole = '-c3';              
                             } 
                             size = $("input[type='hidden'][id$='param-plus-deduct-size"+personRole+"']").val();
                             deductList = $("input[type='hidden'][id$='param-plus-deduct-list"+personRole+"']").val(); 
                             
                             var deductArray = deductList.split(','); 
                             
                             var progressBarWidth = $('#progress-bar-box-div'+personRole).width();
                             var perProgressBarWidth = parseInt(progressBarWidth/size)-2;
                             
                             var centerPosition = parseInt(progressBarWidth/(size*2));                             
                             var selectedDeduct = $("input[type='hidden'][id$='param-plus-deduct-init"+personRole+"']").val();
                             var oleft = 0;
                             var oindex = 0;
                                                          
                             $('#progress-bar-bt-div'+personRole).css('margin-left',(centerPosition-12)+'px');
                             $('#progress-bar-bgcolor-div'+personRole).css('margin-left',(centerPosition)+'px');
                             $('#progress-bar-bgcolor-div'+personRole).css('width',perProgressBarWidth*(size-1)+'px');   
                             
                             for(var i=0; i<deductArray.length; i++){
                                 if(selectedDeduct!=null){
                                     if(deductArray[i] == selectedDeduct){
                                         oindex = i;
                                         oleft = perProgressBarWidth*i;
                                         $('#progress-bar-bt-div'+personRole).css('left',oleft);
                                     }
                                 }
                                 
                                 var formatDeduct = deductArray[i];
                                 if (Cigna.Util.isIOS() || Cigna.Util.isAndroid()) {                       
                                     $('#'+deductArray[i]+'-coordinate'+personRole).css('margin-top','7px');  
                                     if(formatDeduct.length>3){
                                         formatDeduct = deductArray[i].substring(0,deductArray[i].length-3)+'K';
                                     }                               
                                 } else {
                                     if(formatDeduct.length>3){
                                         formatDeduct = deductArray[i].substring(0,deductArray[i].length-3)+','+deductArray[i].substring(deductArray[i].length-3,deductArray[i].length);
                                     }
                                 }
                                 $('#'+deductArray[i]+'-inactive-label'+personRole).text(formatDeduct);
                                 if (Cigna.Util.isMobile()) {
                                     $('#'+deductArray[i]+'-inactive-label'+personRole).css('font-size','13px');
                                 }
                                 
                                 $('#'+deductArray[i]+'-inactive-label-div'+personRole).css('width',perProgressBarWidth+'px');
                                 $('#'+deductArray[i]+'-coordinate'+personRole).css('left',perProgressBarWidth*i+(centerPosition-6)-12*i);                                 
                             }       
                             
                             //init progress bar function
                             (function($){
                                 var movestart = 'mousedown';
                                 if(Cigna.Util.isIOS() || Cigna.Util.isAndroid()){
                                     movestart = 'touchstart';
                                 }
                                 
                                 var move = 'mousemove';
                                 if(Cigna.Util.isIOS() || Cigna.Util.isAndroid()){
                                     move = 'touchmove';
                                 }
                                 
                                 var moveend = 'mouseup';
                                 if(Cigna.Util.isIOS() || Cigna.Util.isAndroid()){
                                     moveend = 'touchend';
                                 }                                
                                                                                                  
                                 //personRole = "{!selectedPersonRole}"; 
                                 var personRole = $("input[type='hidden'][id$='param-selected-person-role']").val(); 
                                 var size = "{!PlusDeductSize}";
                                 var bgIndex = "{!selectedDeductIndex}";                                                   
                                 if(personRole == 'a2' || personRole == 'A2'){
                                     personRole = '-a2';
                                 } else if(personRole == 'a1' || personRole == 'A1'){
                                     personRole = '-a1';
                                 } else if(personRole == 'c1' || personRole == 'C1'){
                                     personRole = '-c1';
                                 } else if(personRole == 'c2' || personRole == 'C2'){
                                     personRole = '-c2';
                                 } else if(personRole == 'c3' || personRole == 'C3'){
                                     personRole = '-c3';
                                 } 
                                 size = $("input[type='hidden'][id$='param-plus-deduct-size"+personRole+"']").val();
                                 
                                 var $box = $('#progress-bar-box-div'+personRole);
                                 var $bg = $('#progress-bar-bg-div'+personRole);
                                 var $bgcolor = $('#progress-bar-bgcolor-div'+personRole);
                                 var $btn = $('#progress-bar-bt-div'+personRole);
                                 var progressBarWidth = $('#progress-bar-bgcolor-div'+personRole).width();
                                 
                                 var statu = false;
                                 var ox = 0;
                                 var lx = 0;
                                 var left = 0;
                                 var bgleft = 0;
                                 
                                 var nowPosition = $('#progress-bar-bt-div'+personRole).css('left').replace('px','');
                                 if(nowPosition>0){
                                     left = parseInt(Number(nowPosition)+6);
                                     bgleft = left;
                                 }
                                 
                                 function onMoveEnd(personRole){
                                     if(statu){                                                                                  
                                         //personRole = "{!selectedPersonRole}";   
                                         var size = '';
                                         var deductList = '';
                                         
                                         if(personRole == 'a2' || personRole == 'A2'){
                                             personRole = '-a2';
                                         } else if(personRole == 'a1' || personRole == 'A1'){
                                             personRole = '-a1';                             
                                         } else if(personRole == 'c1' || personRole == 'C1'){
                                             personRole = '-c1';
                                         } else if(personRole == 'c2' || personRole == 'C2'){
                                             personRole = '-c2';
                                         } else if(personRole == 'c3' || personRole == 'C3'){
                                             personRole = '-c3';
                                         } 
                                         size = $("input[type='hidden'][id$='param-plus-deduct-size"+personRole+"']").val();
                                         deductList = $("input[type='hidden'][id$='param-plus-deduct-list"+personRole+"']").val();                                           
                                         
                                         if(!Cigna.Util.isIOS() & !Cigna.Util.isAndroid()){
                                            //CR-56_Link_20220214
                                            if(personRole==''){
                                                $("input[id*='Quote-PlusDeductibleAmount']").trigger("focus"); 
                                            }else{
                                                $("input[id*='Separated"+personRole+"-PlusDeductibleAmount']").trigger("focus");
                                            }
                                         }
                                         
                                         var perLength = parseInt(progressBarWidth/(size-1));
                                         var halfPerLength = parseInt(progressBarWidth/((size-1)*2));
                                         var match = false;
                                         var index = 0;
                                         var a = deductList.split(',');  
                                         
                                             for(var i=0; i<size; i++){
                                                 if(i == 0){
                                                     if(left<halfPerLength ){                                                         
                                                         if(personRole==''){
                                                            $("input[id*='Quote-PlusDeductibleAmount']").val(a[0]);
                                                         }else{
                                                            $("input[id*='Separated"+personRole+"-PlusDeductibleAmount']").val(a[0]);
                                                         }
                                                         //$("input[id*='Quote-PlusDeductibleAmount"+personRole+"']").val(a[0]);
                                                         match = true;
                                                         left = 0;
                                                         index = 0;
                                                     }
                                                 } else if(i == size-1){
                                                     if(left>=(progressBarWidth - halfPerLength)){
                                                         if(personRole==''){
                                                            $("input[id*='Quote-PlusDeductibleAmount']").val(a[i]);
                                                         }else{
                                                            $("input[id*='Separated"+personRole+"-PlusDeductibleAmount']").val(a[i]);
                                                         }
                                                         //$("input[id*='Quote-PlusDeductibleAmount"+personRole+"']").val(a[i]);
                                                         match = true;
                                                         left = perLength*i;
                                                         index = i;
                                                     }
                                                 } else{
                                                     if(left>=(perLength*i-halfPerLength) && left<(perLength*i+halfPerLength)){
                                                         if(personRole==''){
                                                            $("input[id*='Quote-PlusDeductibleAmount']").val(a[i]);
                                                         }else{
                                                            $("input[id*='Separated"+personRole+"-PlusDeductibleAmount']").val(a[i]);
                                                         }
                                                         //$("input[id*='Quote-PlusDeductibleAmount"+personRole+"']").val(a[i]);
                                                         match = true;
                                                         left = perLength*i;
                                                         index=i;
                                                     }
                                                 }
                                             }
                                             if(!match){
                                                 left = bgleft;
                                                 index = bgIndex;
                                             }
                                         
                                         
                                             
                                         bgleft = left;
                                         bgIndex = index;
                                         
                                         for(var i=0; i < a.length; i++){
                                             $('#'+a[i]+'-inactive-label'+personRole).css('color','#C8C9C7');
                                         }
                                         $('#'+a[index]+'-inactive-label'+personRole).css('color','transparent');
                                         
                                         var formatDeduct = a[index];
                                         if(formatDeduct.length>3){
                                             formatDeduct = a[index].substring(0,a[index].length-3)+','+a[index].substring(a[index].length-3,a[index].length);
                                         }
                                         $('#deduct-active-label'+personRole).text('HKD$'+formatDeduct);
                                         var activeLabelWidth = $('#deduct-active-label'+personRole).width();
                                         
                                         
                                         if(index == 0){
                                             $('#coordinate-active'+personRole).css('padding-left',0);
                                             $('#coordinate-active'+personRole).css('text-align','left');
                                         } else if(index == size-1){
                                             $('#coordinate-active'+personRole).css('padding-left',0);
                                             $('#coordinate-active'+personRole).css('text-align','right');
                                         } else {
                                             $('#coordinate-active'+personRole).css('padding-left',halfPerLength+perLength*index-parseInt(activeLabelWidth/2));
                                             $('#coordinate-active'+personRole).css('text-align','left');
                                         }
                                         
                                         $('#deduct-active-label'+personRole).css('color','#39B54A');
                                         $btn.css('left',left);
                                         //$("input[id*='Quote-PlusDeductibleAmount']").val(a[index]).trigger('change');
                                         ServerQuote.onSelectPlusDeductEnd();
                                         $("input[id*='Quote-PlusDeductibleAmount"+personRole+"']").trigger('change');
                                     }
                                     statu = false; 
                                 }
                                 
                                 $btn.on(movestart,function(e){
                                     
                                     lx = $btn.offset().left;
                                     
                                     var pageX;                                     
                                     if(move == 'mousemove'){
                                             pageX = e.pageX;
                                     } else if(move == 'touchmove'){
                                             pageX = e.originalEvent.targetTouches[0].pageX;
                                     }                                     
                                        
                                     ox = pageX - left;
                                     //bgleft = left;
                                     statu = true;                                     
                                 });
                                 
                                 $box.on(moveend,function(e){
                                      var boxid = $box.attr('id');
                                      var personRole = '';
                                      if(boxid.indexOf('a2')>0){
                                            personRole = 'a2';
                                      } else if(boxid.indexOf('a1')>0){
                                            personRole = 'a1';
                                      } else if(boxid.indexOf('c1')>0){
                                            personRole = 'c1';
                                      } else if(boxid.indexOf('c2')>0){
                                            personRole = 'c2';
                                      } else if(boxid.indexOf('c3')>0){
                                            personRole = 'c3';
                                      }
                                      onMoveEnd(personRole);                              
                                 });
                                 
                                 if(!Cigna.Util.isIOS() & !Cigna.Util.isAndroid()){
                                     $box.on('mouseleave',function(e){
                                          var boxid = $box.attr('id');
                                          var personRole = '';
                                          if(boxid.indexOf('a2')>0){
                                                personRole = 'a2';
                                          } else if(boxid.indexOf('a1')>0){
                                                personRole = 'a1';
                                          } else if(boxid.indexOf('c1')>0){
                                                personRole = 'c1';
                                          } else if(boxid.indexOf('c2')>0){
                                                personRole = 'c2';
                                          } else if(boxid.indexOf('c3')>0){
                                                personRole = 'c3';
                                          } 
                                          onMoveEnd(personRole);                        
