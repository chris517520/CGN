<!--Please change apex page title-->
<apex:page docType="html-5.0" sidebar="false" showHeader="false" title="{!$Label.Get_a_Quote}" controller="GetQuoteCtrl">
    <head>
        <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, viewport-fit=cover"/>
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <style>
            <!--additional inline css-->
            .checkResult{color:red;}
            .bs .form-control:focus {
                box-shadow: none !important;
                border-bottom: 1px solid #eac048 !important;
            }

            .bs input[type="text"], .bs input[type="tel"], .bs input[type="date"], .bs span.form-control  {
                border-top: 0 !important;
                border-left: 0 !important;
                border-right: 0 !important;
                border-bottom: 1px solid #58595B !important;
                background: transparent;
                width: 100%;
                box-shadow: none !important;
                border-radius: 0 !important;
            }
            
            .bs .cigna-quote-question {
                padding-top: 20px;
                padding-bottom: 20px;
                border-bottom: 1px solid #D9D9D6;
            }
            
            /*daft add*/
            .content-active {
                color:#39B54A;
            }
            
            .bs .cigna-nel-deductible-question {
                padding-top: 20px;
                padding-bottom: 20px;
                border-bottom: 1px solid #D9D9D6;
            }
            
            .bs .cigna-question-extra-padding {
                padding-bottom: 100px; 
                /*For Panel */
            }
            .bs .cigna-question-input-fake {
                border-bottom: 1px solid #58595B !important;
                min-width: 50px;
                display: inline-block;
            }
            
            .bs .cigna-answer-wrapper {
                margin-top: 20px;
                margin-left: 30px;    
            }
            .bs .cigna-answer-wrapper > p {
                background: #fff;
            }
            /* Rancho_JOBR324_20230905 */
            .bs .cigna-answer-wrapper > p > label, .bs .cigna-answer-wrapper > span > p > label {
                margin-left: 10px;
                margin-top: 5px;
                font-weight: normal;
                padding: 5px;
                width: 100%;
            }

            .bs input.cigna-question-text {
                border: 0;
                border-bottom: 1px solid #58595B;
                background: transparent;
                width: 100%;
                box-shadow: none !important;
                border-radius: 0;
                color: #58595B;
                width: 50px;
                text-align: center;
            }
            .bs input.cigna-question-text:focus {
                box-shadow: none !important;
                border-bottom: 1px solid #eac048 !important;
            }
            .bs .cigna-layer-card {
                margin: 5px 0px 5px 0px;
                display: block;
                cursor: pointer;
            }
            .bs .cigna-layer-card:hover {
                text-decoration: none;
            }
            .bs .cigna-layer-card > div {
                padding: 10px 15px;
            }
            .bs .cigna-layer-card-desc {
                font-size: 0.9em;
            }
            .bs .cigna-layer-card-desc.cigna-fg-gray-dark {
                border-left: 1px solid #C8C9C7;
                border-right: 1px solid #C8C9C7;
                border-bottom: 1px solid #C8C9C7;
            }
            .bs .cigna-layer-card-head.cigna-bg-gray-3:before {
                font: normal normal normal 14px/1 FontAwesome;
                display: inline-block;
                content: "\f058";
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #C8C9C7;
                font-size: 1.2em;
            }
            .bs .cigna-layer-card-head.cigna-bg-green-medium:before {
                font: normal normal normal 14px/1 FontAwesome;
                display: inline-block;
                content: "\f058";
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #fff;
                font-size: 1.2em;
            }
            
            .bs .cigna-quote-summary {
                width: 100%;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px 0px;
            }
            
            .bs .cigna-quote-summaryTop {
                width: 100%;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px 0px;
                position: fixed;
                top: 50px;
                height: 9999px;
            }
            
            .bs .cigna-quote-amount {
                margin: 0 !important;
                display: table;
                table-layout: fixed;
                width: 100%;
            }
            .bs .cigna-quote-amount > div {
                display: table-cell;
                float: none !important;
            }
            .bs .cigna-quote-amount div{
                font-size: 0.9em;
                font-weight: bold;
                vertical-align: text-top;
            }
            .bs .cigna-quote-amount span {
                font-size: inherit;
                font-weight: bold;
                vertical-align: text-top;
            }
            .bs .cigna-quote-question {
                counter-reset: numOfKid;
            }
            .bs .question-kid-age-label:after {
                counter-increment: numOfKid;
                content: " " counter(numOfKid);
            }
            .bs .cigna-plan-bullet {
                font-size: 0.9em;
                line-height: 1.5;
                margin-top: -10px;
                z-index: -10;
            }
            .bs .cigna-quote-summary-desktop {
                position: relative;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px;
                padding-bottom: 40px;
            }
            
            @media (min-width: 768px) {
                .bs .cigna-floating-bottom-panel {
                    position: relative;
                }
            }
            @media (max-width: 767px) {
                .bs .cigna-floating-bottom-panel {
                    position: fixed;
                    bottom: 0;
                    left: 0;
                    width: 100%;
                }
            }


            .borderUp{
                border-top:1px solid #aaa;
            }

            .borderDown{
                border-bottom:1px solid #aaa;
            }
            
            label.error{color: #337ab7;}
            
            .bs input[type="radio"].cigna-question-radio-diabetes,.bs input[type="radio"].cigna-question-radio1 {
                display: none;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes ~ label,.bs input[type="radio"].cigna-question-radio1 ~ label {
                cursor: pointer;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes + label:before,.bs input[type="radio"].cigna-question-radio1 + label:before {
                font: normal normal normal 14px/1 FontAwesome;
                content: "\f058";
                display: inline-block;
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #C8C9C7;
                font-size: 1.2em;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes:checked + label:before,.bs input[type="radio"].cigna-question-radio1:checked + label:before {
                color: #39B54A;
                font: normal normal normal 14px/1 FontAwesome;
                content: "\f058";
                font-size: 1.2em;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes:checked + label:before,.bs input[type="radio"].cigna-question-radio1:checked + label:before {
                font-weight: bold;
            }
            
            .progress-bar-box{
                position: relative; 
                width: 100%; 
                height: 70px; 
                border: none; 
                margin:50px auto 0;
            }
            .progress-bar-bg{
                height: 5px; 
                margin-top: 19px; 
                border: none; 
                border-radius: 5px; 
                overflow: hidden;
            }
            .progress-bar-bgcolor{
                background:#C8C9C7;
                width: 100%; 
                height:5px;
                //border-radius: 10px;
            }
            .progress-bar-bt{
                width: 24px; 
                height: 24px; 
                background: #39B54A no-repeat center center; 
                border-radius: 15px; 
                overflow: hidden; 
                position: absolute; 
                left: 0px; 
                margin-left: 0px; 
                top: -9px;  
                cursor: pointer;
                //box-shadow:0 0 5px 0 #000 inset;
            }
            .progress-bar-unselectable { 
                -moz-user-select: -moz-none; 
                -khtml-user-select: none; 
                -webkit-user-select: none; 
                -ms-user-select: none; 
                user-select: none; 
            }

            .bs .fill-left{
                padding-left:0 !important;
            }

            .bs .fill-right{
                padding-right:0 !important;
            }
            
            div#overlayLevy{
                display: none;
                z-index: 10;
                background: #D9D9D6;
                position: fixed;
                width: 100%;
                height: 100%;
                top: 0px;
                left: 0px;
                text-align: left;
            }
            
            div#specialBoxLevy{
                display: none;
                position: absolute;
                z-index: 11;
                margin: 20% auto 0px auto;
                width: 80%;
                background: #FFF;
                color: #000;
                left: 10%;
                padding:20px;
                border-radius:5px;
            }
          
            #closeOverlayLevy{
                position: absolute;
                top: 10px;
                right: 10px;
            }
            
            #tccontentLevy {
                padding:5px;
                color:#000;
                font-size:14px;
                width: 100%;
                height: 90%;
                overflow-y:auto;
            }

            /*CR-56_Link_20220214*/
            .ui-tooltip {
                background: black !important;
                max-width: 250px !important;
                width: 250px; 
                min-width: 100px;
                color: white !important;
                z-index: 1070;
                display: block;
                text-align: center !important;
                font-size: 12px !important;
                border-radius: 5px !important;
            }
        </style>
        <script>
            var changeLanguageWarningRequired = true;
        </script>
    </head>
    <apex:define name="additionalLibraries">
         <!--additional css, bootstrap, jquery,js libraries-->
         <!-- CR-56_Link_20220214 -->
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, 'jquery.validate_new.min.js')}"/>
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, 'additional-methods.min.js')}" />
         <apex:outputPanel rendered="{!$Label.User_Language == 'zh_tw'}" layout="none">
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, '/localization/messages_zh_TW.min.js')}" />
         </apex:outputPanel>
         <apex:stylesheet value="{!URLFOR($Resource.Cigna_HK_Icons, 'Cigna-HK-Icons.css')}"/>
    </apex:define>
         
    
    <!-- Questions -->
    <apex:form id="frmGetQuote">
    <apex:actionFunction action="{!refreshPage}" name="refreshPage" reRender="riderPanel" status="loading" />
    
    <apex:actionFunction action="{!refreshPlusDeduct}" name="refreshPlusDeduct" namespace="ServerQuote" reRender="param-panel,PlusdeductPanel" status="loader-icon"/>         
     
    <apex:actionFunction action="{!refreshPlusDeduct}" name="onSelectPlusDeductEnd" namespace="ServerQuote" oncomplete="Cigna.Quote.getQuoteAmount();"/>
    <apex:actionFunction action="{!getCustomerBasic}" name="getCustomerBasic" reRender="personDetailPanel" status="loader-icon" oncomplete="Cigna.Quote.checkEligibility();ServerQuote.InitAppLink();"/>

    <apex:actionStatus id="loading">
        <apex:facet name="start">
        <div style="position: fixed; top: 0; left: 0; right: 0; bottom: 0; opacity: 0.75; z-index: 1000; background-color: black;">
            &nbsp;
        </div>
        <div style="position: fixed; left: 0; top: 0; bottom: 0; right: 0; z-index: 1001; margin: 30% 50%">
            <img class="bs" src="{!URLFOR($Resource.Spinners, 'slds_spinner.gif')}" width="80px" />
        </div>
        </apex:facet>
    </apex:actionStatus>
    

    <!-- AppLink Daft Add -->
    <!-- Start of Confirmation Page -->
    <div class="bs content-step col-xs-12 form-field" data-step="2" style="display:none; padding-top:20px !important;" id="confirmationPage"> <!--  display:none; -->
        <div class="bs col-xs-12 col-sm-10 col-sm-offset-1">
            <div class="col-xs-12 col-sm-12" style="text-align:center;">
                <i class="fa fa-check-circle-o content-active" style="padding-top:30px;font-size:68px;" aria-hidden="true"></i>
            </div>
            <div style="padding-top:100px;text-align:center;font-size:20px;"> <!-- class="col-xs-12 col-sm-12"  -->
                <apex:outputText value="{!$Label.AppLink_Thanks_And_Broker_will_reaching}" escape="false" />
            </div>
        </div>  
        <div class="bs col-xs-3 hidden-xs" />
        <div class="bs col-xs-12 ">
            <div style="padding:20px;"></div>
            <!--desktop action button-->
            <div id="confirmbtnsm" class="bs hidden-xs col-sm-4 col-sm-offset-4 cigna-fg-white cigna-bg-orange-dark desktop-action-btn onclick-pointer" onclick="Cigna.Util.navigateToVFP('GetProtectedVFP', null, null, true);">
                {!$Label.Back}


            </div>
                
            <!--mobile action button-->    
            
            <div id="mobile-app-padding" class="bs row" style="display:none; padding-bottom:100px;">
                <div class="bs col-xs-12"></div>
            </div>
            
           
            
        </div>
    </div>
    <!-- End of Confirmation Page -->

    <apex:outputPanel layout="block" styleClass="bs row" id="quote-back-btn-div">
        <div class="bs col-xs-12">
            <p><a id="quote-back-btn" class="bs cigna-fg-orange-medium onclick-pointer"><i class="ci-hk-arrow-29" aria-hidden="true"></i> {!$Label.Back}</a></p>
        </div>
    </apex:outputPanel>
    <apex:outputPanel layout="block" styleClass="bs row" id="personDetailPanel">
        <div class="bs col-xs-12 cigna-fg-green-medium">
            <h4>{!$Label.QUOTATION_FOR}</h4>
        </div>
        <div class="bs col-xs-12 cigna-fg-gray-dark">
            <h3>
                <apex:outputText value="{0} ({1}, {2})" rendered="{!!showPHfullName}">
                    <apex:param value="{!txn.Policy_Holder_First_Name__c & ' ' & txn.Policy_Holder_Last_Name__c}" />
                    <apex:param value="{!IF($Label.Header_Language_Code == 'zh_TW',IF(txn.Gender__c=='Male','Male','Female'),IF(txn.Gender__c=='Male','男性','女性'))}" />
                    <apex:param value="{!PHage}" />
                </apex:outputText>
                
                <apex:outputText value="{0} ({1}, {2})" rendered="{!showPHfullName}">
                    <apex:param value="{!PHfullName}"/>                            
                    <apex:param value="{!IF($Label.Header_Language_Code == 'zh_TW',IF(txn.Gender__c=='Male','Male','Female'),IF(txn.Gender__c=='Male','男性','女性'))}" />
                    <apex:param value="{!PHage}" />
                </apex:outputText>
            </h3>
        </div>
        <apex:inputHidden value="{!hasEligibility}" id="param-has-eligibility" />
    </apex:outputPanel>
    <div class="bs col-xs-12 cigna-fg-gray-dark"  id="no-permission-warning">
    </div>
    
    <div class="bs row cigna-question-list">
        <div class="bs col-xs-12 col-sm-6 col-lg-7" id="question-List">
    
            <!-- add by David 07/11/2017 for DMB CR start -->
            <apex:outputPanel rendered="{! isDiabetes}" id="qoute-amount-will-include-the-loading">
                <br/>
                <p style="color:#337ab7;">{!$Label.Qoute_amount_will_include_the_loading}</p>
            </apex:outputPanel>
            <apex:outputPanel id="loadingMsg" rendered="{! isDiabetes}">
                <div id="someDeclineMsg" style="display:none;">
                <p style="color:#337ab7;">{!$Label.Qoute_Some_application_decline}</p>
                </div>
                <div id="allDeclineMsg" style="display:none;">
                <p style="color:#337ab7;">{!$Label.Qoute_All_application_decline}</p>
                </div>
            </apex:outputPanel>
        
            <!-- add by David 07/11/2017 for DMB CR end --> 
            
            <!--For DPSP-839-->
            <apex:outputPanel id="disable_buy_now_message">
                <apex:outputPanel rendered="{!DisableBuyNowbyPremium}">
                    <div>
                    <p style="color:#337ab7;">{!$Label.Quote_Disable_Buy_Now_Notification_Message}</p>
                    </div>
                </apex:outputPanel>
            </apex:outputPanel>               
            
            <!--- Adult number and child number selection panel start-->
            <apex:outputPanel rendered="{!hasAdultSelectOption}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">

                <apex:outputPanel id="seletedNumOfAdult"><h4><span class="bs cigna-question-input-fake">{! IF(numOfAdult==1,$Label.Qoute_One,IF(numOfAdult==2,$Label.Qoute_Two,IF(numOfAdult==0,$Label.Qoute_No,'')))}&nbsp;</span>{!$Label.Quote_adult_will_be_covered_in_this_plan}</h4></apex:outputPanel>

                <br/>
                <div id="num-of-adult-question" class="bs cigna-answer-wrapper">  
                    <p class="bs col-xs-6 col-sm-6" style="padding-left: 0px;">
                        <input type="radio" id="question-ans-adult-01" value="0" name="question-ans-adult" class="bs cigna-question-radio" />
                        <label for="question-ans-adult-01">{!$Label.Qoute_No}</label>
                    </p>
                    <p class="bs col-xs-6 col-sm-6">
                        <input type="radio" id="question-ans-adult-02" value="1" name="question-ans-adult" class="bs cigna-question-radio" />
                        <label for="question-ans-adult-02">{!$Label.Qoute_One}</label>
                    </p>
                    <apex:inputHidden value="{!numOfAdult}" id="Quote-numAdults" html-class="cigna-validation" />
                </div>

            </apex:outputPanel>
                        
            <!--For Not Diabetes-->
            <apex:outputPanel rendered="{!hasChildOption}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">
                <h4>{!$Label.Quote_Question_The_Plan} <span class="bs cigna-question-input-fake">&nbsp;</span> {!$Label.Quote_Question_Cover_Kids}</h4><br/>
                <div id="param-cover-child" class="bs cigna-answer-wrapper">
                    <p class="bs col-xs-6 col-sm-6" style="padding-left: 0px;">
                        <input type="radio" id="question-ans-02-01" value="true" name="question-ans-02" class="bs cigna-question-radio" />
                        <label for="question-ans-02-01">{!$Label.Quote_Answer_Will}</label>
                    </p>
                    <p class="bs col-xs-6 col-sm-6">
                        <input type="radio" id="question-ans-02-02" value="false" name="question-ans-02" class="bs cigna-question-radio"/>
                        <label for="question-ans-02-02">{!$Label.Quote_Answer_Won_t}</label>
                    </p>
                    <apex:inputHidden value="{!paramCoverChild}" id="Quote-CoverChild" html-class="cigna-validation" />
                </div>
            </apex:outputPanel>
            <input style="display:none;" id="Cover-Child-Num" />
            <apex:outputPanel rendered="{!hasChildOption}" layout="block" id="quote-num-of-kids-selector" html-data-id="quote-num-of-kids-selector" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">
                <h4 class="bs">{!$Label.Quote_Answer_The_plan_will_cover} 
                    <select id="quote-question-num-kids" class="bs form-field-style-select" style="display: inline; width: auto;">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                    </select> 
                    {!$Label.Quote_Question_Kids}
                </h4><br/>
                <apex:inputHidden value="{!coverChildNum}" id="Cover-Child-Count" />
                <br/>
            </apex:outputPanel>
            <apex:outputPanel id="diff-plan-options-hkaa-panel">
            <div id="diff-plan-options-hkaa-div">
                <!--<apex:outputPanel rendered="{!hasDiffPlanOptions_hkaa && (coverChildNum != null && coverChildNum > 0) }" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark  col-xs-12" >-->
                <apex:outputPanel rendered="{!hasDiffPlanOptions_hkaa}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark  col-xs-12" >
                    <h4 class="bs h4fontsize">{!$Label.Quote_Question_Different_Plan_and_Rider}</h4>
                    <br/>
                    <div id="param-choose-different-plan-rider" class="bs cigna-answer-wrapper">  
                        <p class="bs col-xs-6 col-sm-6" style="padding-left: 0px;">
                            <input type="radio" id="question-ans-plan-rider-hkaa-01" value="true" name="question-ans-plan-rider-hkaa" class="bs cigna-question-radio plan-info-input-label-checker" />
                            <label for="question-ans-plan-rider-hkaa-01">{!$Label.Yes}</label>
                        </p>
                        <p class="bs col-xs-6 col-sm-6">
                            <input type="radio" id="question-ans-plan-rider-hkaa-02" value="false" name="question-ans-plan-rider-hkaa" class="bs cigna-question-radio plan-info-input-label-checker" />
                            <label for="question-ans-plan-rider-hkaa-02">{!$Label.No}</label>
                        </p>
                        <apex:inputHidden value="{!isPlanDifferentOption}" id="Quote-Is-Different-Plan" html-class="cigna-validation"/>
                    </div>
                </apex:outputPanel>
            </div>
            </apex:outputPanel>
            
            <apex:outputPanel id="Adult-1-Detail-hkaa-Panel" rendered="{!!hasAdultSelectOption}" styleClass="bs col-xs-12" style="padding:0 0 20px 0;border-bottom: 1px solid #D9D9D6;">                
                <apex:outputPanel rendered="{!hasSmokerOption && !hasAdultSelectOption}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12" style="border-bottom: none;padding-bottom:0px;">
                   
                    <apex:outputPanel rendered="{!isCustomerPortal}"><h4>{!$Label.I} <span class="bs cigna-question-input-fake">&nbsp;</span> {!$Label.Smoke}.</h4></apex:outputPanel>
                    <apex:outputPanel rendered="{!!isCustomerPortal}"><h4>{!$Label.Does_your_client_smoke}</h4></apex:outputPanel>
                    <br/>
                    <div id="param-is-smoker" class="bs cigna-answer-wrapper">  
                        <p class="bs col-xs-6 col-sm-6" style="padding-left: 0px;">
                            <input type="radio" id="question-ans-01-01" value="true" name="question-ans-01" class="bs cigna-question-radio" />
                            <label for="question-ans-01-01">{!IF(isCustomerPortal, $Label.Quote_Answer_Do, $Label.Yes)}</label>
                        </p>
                        <p class="bs col-xs-6 col-sm-6">
                            <input type="radio" id="question-ans-01-02" value="false" name="question-ans-01" class="bs cigna-question-radio" />
                            <label for="question-ans-01-02">{!IF(isCustomerPortal, $Label.Quote_Answer_Dont, $Label.No)}</label>
                        </p>
                        <apex:inputHidden value="{!paramIsSmoker}" id="Quote-IsSmoker" html-class="cigna-validation" /> 
                    </div>
                </apex:outputPanel> 
                                    
                <apex:outputPanel id="Adule-1-Plan-Panel-diff-plan" rendered="{!isNEL}">            
                    <!--<apex:outputPanel rendered="{! isPlanDifferentOption == 'true' }">-->
                    <div id="Adule-1-Plan-Panel-diff-plan-div">
                        <apex:outputPanel rendered="{!hasAreaOfCoverage}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12" style="border-bottom:none;padding-bottom:0px;">
                                <h4>{!$Label.The_area_of_coverage_would_be} <span class="bs cigna-question-input-fake">&nbsp;</span>{!IF($Label.User_Language == 'zh_tw','。','.')}</h4><br/>
                                <div id="param-area-of-coverage-a1" class="bs cigna-answer-wrapper">
