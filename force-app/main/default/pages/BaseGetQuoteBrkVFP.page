<apex:page docType="html-5.0" sidebar="false" showHeader="false" title="{!$Label.Get_a_Quote}" controller="GetQuoteCtrl">
    <head>
        <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width"/>
        <style>
            <!--additional inline css-->
            .checkResult{color:red;}
            .bs .form-control:focus {
                box-shadow: none !important;
                border-bottom: 1px solid #eac048 !important;
            }

            .bs input[type="text"], .bs input[type="tel"], .bs input[type="date"], .bs span.form-control  {
                border-top: 0 !important;
                border-left: 0 !important;
                border-right: 0 !important;
                border-bottom: 1px solid #58595B !important;
                background: transparent;
                width: 100%;
                box-shadow: none !important;
                border-radius: 0 !important;
            }
            
            .bs .cigna-quote-question {
                padding-top: 20px;
                padding-bottom: 20px;
                border-bottom: 1px solid #D9D9D6;
            }

            .bs .cigna-quote-question-adult {
                padding-top: 20px;
                padding-bottom: 20px;
                border-bottom: 1px solid #D9D9D6;
            }
            
            .bs .cigna-nel-deductible-question {
                padding-top: 20px;
                padding-bottom: 20px;
                border-bottom: 1px solid #D9D9D6;
            }
            
            .bs .cigna-question-extra-padding {
                padding-bottom: 100px; 
                /*For Panel */
            }
            .bs .cigna-question-input-fake {
                border-bottom: 1px solid #58595B !important;
                min-width: 50px;
                display: inline-block;
            }
            
            .bs .cigna-answer-wrapper {
                margin-top: 20px;
                margin-left: 30px;    
            }
            .bs .cigna-answer-wrapper > p {
                background: #fff;
            }
            /* Rancho_JOBR324_20230905 */
            .bs .cigna-answer-wrapper > p > label, .bs .cigna-answer-wrapper > span > p > label {
                margin-left: 10px;
                margin-top: 5px;
                font-weight: normal;
                padding: 5px;
                width: 100%;
            }

            .bs input.cigna-question-text {
                border: 0;
                border-bottom: 1px solid #58595B;
                background: transparent;
                width: 100%;
                box-shadow: none !important;
                border-radius: 0;
                color: #58595B;
                width: 50px;
                text-align: center;
            }
            .bs input.cigna-question-text:focus {
                box-shadow: none !important;
                border-bottom: 1px solid #eac048 !important;
            }
            .bs .cigna-layer-card {
                margin: 5px 0px 5px 0px;
                display: block;
                cursor: pointer;
            }
            .bs .cigna-layer-card:hover {
                text-decoration: none;
            }
            .bs .cigna-layer-card > div {
                padding: 10px 15px;
            }
            .bs .cigna-layer-card-desc {
                font-size: 0.9em;
            }
            .bs .cigna-layer-card-desc.cigna-fg-gray-dark {
                border-left: 1px solid #C8C9C7;
                border-right: 1px solid #C8C9C7;
                border-bottom: 1px solid #C8C9C7;
            }
            .bs .cigna-layer-card-head.cigna-bg-gray-3:before {
                font: normal normal normal 14px/1 FontAwesome;
                display: inline-block;
                content: "\f058";
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #C8C9C7;
                font-size: 1.2em;
            }
            .bs .cigna-layer-card-head.cigna-bg-green-medium:before {
                font: normal normal normal 14px/1 FontAwesome;
                display: inline-block;
                content: "\f058";
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #fff;
                font-size: 1.2em;
            }
            
            .bs .cigna-quote-summary {
                width: 100%;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px 0px;
            }
            
            .bs .cigna-quote-summaryTop {
                width: 100%;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px 0px;
                position: fixed;
                top: 50px;
                height: 9999px;
            }
            
            .bs .cigna-quote-amount {
                margin: 0 !important;
                display: table;
                table-layout: fixed;
                width: 100%;
            }
            .bs .cigna-quote-amount > div {
                display: table-cell;
                float: none !important;
            }
            .bs .cigna-quote-amount div{
                font-size: 0.9em;
                font-weight: bold;
                vertical-align: text-top;
            }
            .bs .cigna-quote-amount span {
                font-size: inherit;
                font-weight: bold;
                vertical-align: text-top;
            }
            .bs .cigna-quote-question {
                counter-reset: numOfKid;
            }
            .bs .question-kid-age-label:after {
                counter-increment: numOfKid;
                content: " " counter(numOfKid);
            }
            .bs .cigna-plan-bullet {
                font-size: 0.9em;
                line-height: 1.5;
                margin-top: -10px;
                z-index: -10;
            }
            .bs .cigna-quote-summary-desktop {
                position: relative;
                min-height: 50px;
                background: #D9D9D6;
                color: #58595B;
                padding: 10px;
            }
            
            .bs .quote-action-btn-group {
                display: table;
                //width: 100%;
                table-layout: fixed;
                border-collapse: separate;
                position: relative;
                vertical-align: middle;
                min-height: 40px;
            }
            .bs .quote-action-btn-group > .quote-action-btn {
                display: table-cell;
                width: 1%;
                position: relative;
                padding: 6px 12px;
                margin-bottom: 0;
                font-size: 0.9em;
                font-weight: 400;
                line-height: 1.42857143;
                text-align: center;
                vertical-align: middle;
                cursor: pointer;
                /* Rancho_HKITSFDC264_20250903 - replace "Gotham SSm A", "Gotham SSm B" */
                font-family: "ValueSansRegular", "Microsoft Jhenghei", "LiHei Pro", "Heiti TC", Arial, Helvetica, sans-serif;
                font-style: normal;
                font-weight: 500;
                text-decoration: none;
                word-wrap: break-word;
                white-space: normal;
                 background: #E35205;
            }
            .bs .quote-action-btn-group > .quote-action-btn-retrieve {
                display: table-cell;
                width: 1%;
                position: relative;
                margin-bottom: 0;
                font-size: 0.9em;
                font-weight: 400;
                line-height: 1.42857143;
                text-align: center;
                vertical-align: middle;
                cursor: pointer;
                /* Rancho_HKITSFDC264_20250903 - replace "Gotham SSm A", "Gotham SSm B" */
                font-family: "ValueSansRegular", "Microsoft Jhenghei", "LiHei Pro", "Heiti TC", Arial, Helvetica, sans-serif;
                font-style: normal;
                font-weight: 500;
                text-decoration: none;
                word-wrap: break-word;
                white-space: normal;
                 background: #E35205;
            }
            
            .bs input[type="radio"].cigna-question-radio-diabetes,.bs input[type="radio"].cigna-question-radio1 {
                display: none;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes ~ label,.bs input[type="radio"].cigna-question-radio1 ~ label {
                cursor: pointer;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes + label:before,.bs input[type="radio"].cigna-question-radio1 + label:before {
                font: normal normal normal 14px/1 FontAwesome;
                content: "\f058";
                display: inline-block;
                height: 20px;
                margin: 0 .25em 0 0;
                padding: 0;
                vertical-align: top;
                width: 20px;
                color: #C8C9C7;
                font-size: 1.2em;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes:checked + label:before,.bs input[type="radio"].cigna-question-radio1:checked + label:before {
                color: #39B54A;
                font: normal normal normal 14px/1 FontAwesome;
                content: "\f058";
                font-size: 1.2em;
            }
            .bs input[type="radio"].cigna-question-radio-diabetes:checked + label:before,.bs input[type="radio"].cigna-question-radio1:checked + label:before {
                font-weight: bold;
            }
        
            .bs .h4fontsize {
                font-size:16px;
            }

            @media (min-width: 768px) {
                .bs .cigna-floating-bottom-panel {
                    position: relative;
                }
            }
            @media (max-width: 767px) {
                .bs .cigna-floating-bottom-panel {
                    position: fixed;
                    bottom: 0;
                    left: 0;
                    width: 100%;
                }
            }

            .borderUp{
                border-top:1px solid #aaa;
            }

            .borderDown{
                border-bottom:1px solid #aaa;
            }
            
            .progress-bar-box{
                position: relative; 
                width: 100%; 
                height: 70px; 
                border: none; 
                margin:50px auto 0;
                //cursor: pointer;
            }
            .progress-bar-bg{
                height: 5px; 
                margin-top: 19px; 
                border: none; 
                border-radius: 5px; 
                overflow: hidden;
            }
            .progress-bar-bgcolor{
                background:#C8C9C7;
                width: 100%; 
                height: 5px; 
                //border-radius: 10px;
            }
            .progress-bar-bt{
                width: 24px; 
                height: 24px; 
                background: #39B54A no-repeat center center; 
                border-radius: 15px; 
                overflow: hidden; 
                position: absolute; 
                left: 0px; 
                margin-left: 0px; 
                top: -9px;  
                cursor: pointer;
                //box-shadow:0 0 5px 0 #000 inset;
            }
            .progress-bar-unselectable { 
                -moz-user-select: -moz-none; 
                -khtml-user-select: none; 
                -webkit-user-select: none; 
                -ms-user-select: none; 
                user-select: none; 
            }
        
            label.error{color: #337ab7;}

            .bs .fill-left{
                padding-left:0 !important;
            }

            .bs .fill-right{
                padding-right:0 !important;
            }
            
            div#overlayLevy{
                display: none;
                z-index: 10;
                background: #D9D9D6;
                position: fixed;
                width: 100%;
                height: 100%;
                top: 0px;
                left: 0px;
                text-align: left;
            }
            
            div#specialBoxLevy{
                display: none;
                position: absolute;
                z-index: 11;
                margin: 20% auto 0px auto;
                width: 80%;
                background: #FFF;
                color: #000;
                left: 10%;
                padding:20px;
                border-radius:5px;
            }
          
            #closeOverlayLevy{
                position: absolute;
                top: 10px;
                right: 10px;
            }
            
            #tccontentLevy {
                padding:5px;
                color:#000;
                font-size:14px;
                width: 100%;
                height: 90%;
                overflow-y:auto;
            }

            /*CR-56_Link_20220214*/
            .ui-tooltip {
                background: black !important;
                max-width: 250px !important;
                width: 250px; 
                min-width: 100px;
                color: white !important;
                z-index: 1070;
                display: block;
                text-align: center !important;
                font-size: 12px !important;
                border-radius: 5px !important;
                font-family: "ValueSansRegular";/* Rancho_HKITSFDC264_20250903 - update font */
            }
        </style>
        <script>
            var changeLanguageWarningRequired = true;
        </script>
    </head>
    <apex:define name="additionalLibraries">
         <!--additional css, bootstrap, jquery,js libraries-->
         <!-- CR-56_Link_20220214 -->
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, 'jquery.validate_new.min.js')}"/>
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, 'additional-methods.min.js')}" />
         <apex:outputPanel rendered="{!$Label.User_Language == 'zh_tw'}" layout="none">
         <script type="text/javascript" src="{!URLFOR($Resource.jQuery_Validation, '/localization/messages_zh_TW.min.js')}" />
         </apex:outputPanel>
         <apex:stylesheet value="{!URLFOR($Resource.Cigna_HK_Icons, 'Cigna-HK-Icons.css')}"/>
    </apex:define>
     
    
    <!-- Questions -->
    <apex:form id="frmGetQuote">
    <apex:actionFunction action="{!refreshPage}" name="refreshPage" reRender="riderPanel" status="loading" />
    <apex:actionFunction action="{!refreshPlusDeduct}" name="refreshPlusDeduct" namespace="ServerQuote" reRender="PlusdeductPanel" status="loader-icon"/>
    <apex:actionFunction action="{!refreshPlusDeduct}" name="onSelectPlusDeductEnd" namespace="ServerQuote" reRender="PlusdeductQuestion" status="loader-icon"/>
    <apex:actionFunction action="{!getCustomerBasic}" name="getCustomerBasic" reRender="personDetailPanel" status="loader-icon"/>
    <apex:actionFunction action="{!retrieveA1Detail}" name="retrieveA1Detail" reRender="adult1DetailPanel" status="loader-icon"/>
    <apex:actionFunction action="{!retrieveA2Detail}" name="retrieveA2Detail" reRender="adult2DetailPanel" status="loader-icon"/>
    <apex:actionStatus id="loading">
        <apex:facet name="start">
        <div style="position: fixed; top: 0; left: 0; right: 0; bottom: 0; opacity: 0.75; z-index: 1000; background-color: black;">
            &nbsp;
        </div>
        <div style="position: fixed; left: 0; top: 0; bottom: 0; right: 0; z-index: 1001; margin: 30% 50%">
            <img class="bs" src="{!URLFOR($Resource.Spinners, 'slds_spinner.gif')}" width="80px" />
        </div>
        </apex:facet>
    </apex:actionStatus>
    <!--<apex:actionStatus id="loader-icon">
        <apex:facet name="start">
           <div class="bs col-xs-12 col-lg-12 loading-div" style="padding: 0px;">
              <span class="loading-helper"></span>
              <i class="spinner-icon fa fa-spinner fa-pulse fa-3x fa-fw"></i>
              <span class="sr-only">Loading...</span>
           </div>
        </apex:facet>
    </apex:actionStatus>-->
    <apex:outputPanel layout="block" styleClass="bs row">
        <div class="bs col-xs-12">
            <p><a id="quote-back-btn" class="bs cigna-fg-orange-medium onclick-pointer"><i class="ci-hk-arrow-29" aria-hidden="true"></i> {!$Label.Back}</a></p>
        </div>
    </apex:outputPanel>
    
    <apex:outputPanel id="errmessage">
        <div class="bs row">
            <apex:pagemessages ></apex:pagemessages>
        </div>
    </apex:outputPanel>

    <apex:outputPanel layout="block" styleClass="bs row" id="personDetailPanel">
        <div class="bs col-xs-12 cigna-fg-green-medium">
            <h4>{!$Label.Quotation}</h4>
        </div>
    </apex:outputPanel>
    
    <div class="bs row cigna-question-list">
        <div class="bs col-xs-12 col-sm-7" id="question-List">

        <!-- add by David 07/11/2017 for DMB CR start -->
        <apex:outputPanel rendered="{! isDiabetes}">
            <br/>
            <p style="color:#337ab7;">{!$Label.Qoute_amount_will_include_the_loading}</p>
        </apex:outputPanel>
        <apex:outputPanel id="loadingMsg" rendered="{! isDiabetes}">
            <div id="someDeclineMsg" style="display:none;">
            <p style="color:#337ab7;">{!$Label.Qoute_Some_application_decline}</p>
            </div>
            <div id="allDeclineMsg" style="display:none;">
            <p style="color:#337ab7;">{!$Label.Qoute_All_application_decline}</p>
            </div>
        </apex:outputPanel>
        <!-- Rancho_JOBR365_20240405 - elite 360 broker get quote -->
        <apex:outputPanel rendered="{!selectedProduct=='NEL2'}">
            <br/>
            <p style="color:#337ab7;">{!transField['Elite_360_Get_Quote_Disclaimer'][$Label.Header_Language_Code_Current]}</p>
        </apex:outputPanel>

        <!-- add by David 07/11/2017 for DMB CR end -->    
            
            <!--For DPSP-839-->
            <apex:outputPanel id="disable_buy_now_message">
                <apex:outputPanel rendered="{!DisableBuyNowbyPremium}">
                    <div>
                    <p style="color:#337ab7;">{!$Label.Quote_Disable_Buy_Now_Notification_Message_Broker}</p>
                    </div>
                </apex:outputPanel>
            </apex:outputPanel> 
            
            <!--- Adult number and child number selection panel start-->
            <apex:outputPanel rendered="{!hasAdultSelectOption}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">

                <apex:outputPanel id="seletedNumOfAdult"><h4><span class="bs cigna-question-input-fake">{! IF(numOfAdult==1,$Label.Qoute_One,IF(numOfAdult==2,$Label.Qoute_Two,IF(numOfAdult==0,$Label.Qoute_No,'')))}&nbsp;</span>{!$Label.Quote_adult_will_be_covered_in_this_plan}</h4></apex:outputPanel>

                <br/>
                <div id="num-of-adult-question" class="bs cigna-answer-wrapper">  
                    <p class="bs col-xs-4 col-sm-4" style="padding-left: 0px;">
                        <input type="radio" id="question-ans-adult-01" value="0" name="question-ans-adult" class="bs cigna-question-radio" />
                        <label for="question-ans-adult-01">{!$Label.Qoute_No}</label>
                    </p>
                    <p class="bs col-xs-4 col-sm-4">
                        <input type="radio" id="question-ans-adult-02" value="1" name="question-ans-adult" class="bs cigna-question-radio" />
                        <label for="question-ans-adult-02">{!$Label.Qoute_One}</label>
                    </p>
                    <p class="bs col-xs-4 col-sm-4">
                        <input type="radio" id="question-ans-adult-03" value="2" name="question-ans-adult" class="bs cigna-question-radio" />
                        <label for="question-ans-adult-03">{!$Label.Qoute_Two}</label>
                    </p>
                    <apex:inputHidden value="{!numOfAdult}" id="Quote-numAdults" html-class="cigna-validation" />
                </div>

            </apex:outputPanel>

            <apex:outputPanel rendered="{!hasChildSelectOption}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">

                <apex:outputPanel id="seletedNumOfChild"><h4><span class="bs cigna-question-input-fake">{! IF(numOfChild==1,$Label.Qoute_One,IF(numOfChild==2,$Label.Qoute_Two,IF(numOfChild==3,$Label.Qoute_Three, IF(numOfChild==0,$Label.Qoute_No,''))))}&nbsp;</span>{!$Label.Quote_child_will_be_covered_in_this_plan}</h4></apex:outputPanel>

                <br/>
                <div id="num-of-child-question" class="bs cigna-answer-wrapper">  
                    <p class="bs col-xs-6 col-sm-3" style="padding-left: 0px;">
                        <input type="radio" id="question-ans-child-01" value="0" name="question-ans-child" class="bs cigna-question-radio" />
                        <label for="question-ans-child-01">{!$Label.Qoute_No}</label>
                    </p>
                    <p class="bs col-xs-6 col-sm-3">
                        <input type="radio" id="question-ans-child-02" value="1" name="question-ans-child" class="bs cigna-question-radio" />
                        <label for="question-ans-child-02">{!$Label.Qoute_One}</label>
                    </p>
                    <p class="bs col-xs-6 col-sm-3">
                        <input type="radio" id="question-ans-child-03" value="2" name="question-ans-child" class="bs cigna-question-radio" />
                        <label for="question-ans-child-03">{!$Label.Qoute_Two}</label>
                    </p>
                    <p class="bs col-xs-6 col-sm-3">
                        <input type="radio" id="question-ans-child-04" value="3" name="question-ans-child" class="bs cigna-question-radio" />
                        <label for="question-ans-child-04">{!$Label.Qoute_Three}</label>
                    </p>
                    <apex:inputHidden value="{!numOfChild}" id="Quote-numChildren" html-class="cigna-validation" />
                </div>

            </apex:outputPanel>
            <!--- Adult number and child number selection panel end-->

            <!--- Adult information panel start-->
            <apex:outputPanel id="adultInfoPanel">
            <apex:outputPanel rendered="{!hasAdultSelectOption && numOfAdult !=null && numOfAdult >0}" layout="block" styleClass="bs cigna-quote-question cigna-fg-gray-dark col-xs-12">
                <apex:outputPanel id="adult-1-detail-panel" rendered="{!numOfAdult==1 || numOfAdult==2}">
                    <apex:outputPanel layout="block">
                        <div class="bs form-group col-xs-12 fill-left fill-right">
                            <div class="bs col-sm-5 fill-left">
                                <apex:outputPanel rendered="{!numOfAdult==1}"><h4 class="bs h4fontsize">{!$Label.Quotation_Age_of_Person_Insured}</h4></apex:outputPanel>
                                <apex:outputPanel rendered="{!numOfAdult==2}"><h4 class="bs h4fontsize">{!$ObjectType.ESales_Transaction__c.Fields.Age_of_Adult_1__c.Label}</h4></apex:outputPanel>
                            </div>
                            <div class="bs col-sm-7 col-xs-12 fill-left" >
                                <apex:input type="tel" id="Adult-Age-a1-DMB" value="{!paramA1age}" styleClass="bs form-control" />
                            </div>
                        </div>
                    </apex:outputPanel>
                        
                    <div class="bs col-sm-12 col-xs-12" style="padding-left:0px;">
                        <div class="bs col-sm-5 col-xs-12" style="padding-left:0px;padding-bottom:20px;">
                            <apex:outputPanel rendered="{!numOfAdult==1}"><h4 class="bs h4fontsize">{!$ObjectType.ESales_Transaction__c.Fields.Height__c.Label}</h4></apex:outputPanel>
                            <apex:outputPanel rendered="{!numOfAdult==2}"><h4 class="bs h4fontsize">{!$Label.Height_of_Adult1}</h4></apex:outputPanel>

                        </div>
                        <div class="bs col-sm-3 col-xs-4" style="padding-left:0px;padding-bottom:20px;">
                            <apex:selectList id="Quote-Height-Unit-PH" styleClass="bs form-field-style-select" size="1" value="{!paramHeightUnit}" multiselect="false" onchange="Cigna.Quote.onselectHeightUnit('PH');">
                                <apex:selectOptions value="{!HeightUnitOption}"/>
                            </apex:selectList>
                        </div>
                        <div class="bs col-sm-4 col-xs-8" id="CMHeightPH">
                            <apex:input type="tel" id="Quote-Height-PH" value="{!paramHeight}" styleClass="bs form-control" html-maxlength="6" />
                        </div>
                        <div class="bs col-sm-4 col-xs-8 row" id="FTHeightPH" style="display:none;">
                            <div class="bs col-sm-6 col-xs-6">
                                <apex:input type="tel" id="Quote-Height1-Feet-PH" value="{!paramHeightFeet}" styleClass="bs form-control" html-maxlength="6" html-placeholder="{!$Label.eSales_Application_Feet}"/>
                            </div>
                            <div class="bs col-sm-6 col-xs-6">
                                <apex:input type="tel" id="Quote-Height2-Inch-PH" value="{!paramHeightInch}" styleClass="bs form-control" html-maxlength="6" html-placeholder="{!$Label.eSales_Application_Inch}"/>
                            </div>
                         </div>
                    </div>
                     
                    <div class="bs col-sm-12 col-xs-12" style="padding-left:0px;">
                         <div class="bs col-sm-5 col-xs-12" style="padding-left:0px;padding-bottom:20px;">
                            <apex:outputPanel rendered="{!numOfAdult==1}"><h4 class="bs h4fontsize">{!$ObjectType.ESales_Transaction__c.Fields.Weight__c.Label}</h4></apex:outputPanel>
                            <apex:outputPanel rendered="{!numOfAdult==2}"><h4 class="bs h4fontsize">{!$Label.Weight_of_Adult1}</h4></apex:outputPanel>
                        </div>
