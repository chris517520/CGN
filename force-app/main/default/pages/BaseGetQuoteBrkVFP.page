                                     function onMoveEnd(){
                                         if(statu){
                                             if(!Cigna.Util.isIOS() & !Cigna.Util.isAndroid()){
                                                //CR-56_Link_20220214
                                                $("input[id*='Quote-PlusDeductibleAmount']").trigger("focus"); 
                                             }
                                             var size = "{!PlusDeductSize}";
                                             var perLength = parseInt(progressBarWidth/(size-1));
                                             var halfPerLength = parseInt(progressBarWidth/((size-1)*2));
                                             var match = false;
                                             var index = 0;
                                             var deductList = "{!PlusDeducts}";
                                             var a = deductList.split(',');  
                                             
                                                 for(var i=0; i<size; i++){
                                                     if(i == 0){
                                                         if(left<halfPerLength ){
                                                             $("input[id*='Quote-PlusDeductibleAmount']").val(a[0]);
                                                             match = true;
                                                             left = 0;
                                                             index = 0;
                                                         }
                                                     } else if(i == size-1){
                                                         if(left>=(progressBarWidth - halfPerLength)){
                                                             $("input[id*='Quote-PlusDeductibleAmount']").val(a[i]);
                                                             match = true;
                                                             left = perLength*i;
                                                             index = i;
                                                         }
                                                     } else{
                                                         if(left>=(perLength*i-halfPerLength) && left<(perLength*i+halfPerLength)){
                                                             $("input[id*='Quote-PlusDeductibleAmount']").val(a[i]);
                                                             match = true;
                                                             left = perLength*i;
                                                             index=i;
                                                         }
                                                     }
                                                 }
                                                 if(!match){
                                                     left = bgleft;
                                                     index = bgIndex;
                                                 }
                                             
                                             
                                                 
                                             bgleft = left;
                                             bgIndex = index;
                                             
                                             for(var i=0; i < a.length; i++){
                                                 $('#'+a[i]+'-inactive-label').css('color','#C8C9C7');
                                             }
                                             $('#'+a[index]+'-inactive-label').css('color','transparent');
                                             
                                             var formatDeduct = a[index];
                                             if(formatDeduct.length>3){
                                                 formatDeduct = a[index].substring(0,a[index].length-3)+','+a[index].substring(a[index].length-3,a[index].length);
                                             }
                                             $('#deduct-active-label').text('HKD$'+formatDeduct);
                                             var activeLabelWidth = $('#deduct-active-label').width();
                                             
                                             
                                             if(index == 0){
                                                 $('#coordinate-active').css('padding-left',0);
                                                 $('#coordinate-active').css('text-align','left');
                                             } else if(index == size-1){
                                                 $('#coordinate-active').css('padding-left',0);
                                                 $('#coordinate-active').css('text-align','right');
                                             } else {
                                                 $('#coordinate-active').css('padding-left',halfPerLength+perLength*index-parseInt(activeLabelWidth/2));
                                                 $('#coordinate-active').css('text-align','left');
                                             }
                                             
                                             $('#deduct-active-label').css('color','#39B54A');
                                             $btn.css('left',left);
                                             //$("input[id*='Quote-PlusDeductibleAmount']").val(a[index]).trigger('change');
                                             ServerQuote.onSelectPlusDeductEnd();
                                             $("input[id*='Quote-PlusDeductibleAmount']").trigger('change');
                                         }
                                         statu = false; 
                                     }
                                     
                                     $btn.on(movestart,function(e){
                                         
                                         lx = $btn.offset().left;
                                         
                                         var pageX;                                     
                                         if(move == 'mousemove'){
                                                 pageX = e.pageX;
                                         } else if(move == 'touchmove'){
                                                 pageX = e.originalEvent.targetTouches[0].pageX;
                                         }                                     
                                            
                                         ox = pageX - left;
                                         //bgleft = left;
                                         statu = true;                                     
                                     });
                                     $box.on(moveend,function(e){
                                          onMoveEnd();                              
                                     });
                                     
                                     if(!Cigna.Util.isIOS() & !Cigna.Util.isAndroid()){
                                         $box.on('mouseleave',function(e){
                                              onMoveEnd();                         
                                         }); 
                                     }
                                     
                                     $box.on(move,function(e) {                                    
                                         if(statu){                                         
                                             var pageX;
                                             if(move == 'mousemove'){
                                                 pageX = e.pageX;
                                             } else if(move == 'touchmove'){
                                                 pageX = e.originalEvent.targetTouches[0].pageX;
                                             }
                                             
                                             left = pageX - ox;
                                             if(left <= 0){
                                                 left = 0;
                                             }
                                             if(left >= progressBarWidth){
                                                 left = progressBarWidth;
                                             }
                                             
                                             $btn.css('left',left);
                                        }
                                    });
                                       
                                 })(jQuery);
                                
                                $("input[id*='Quote-PlusDeductibleAmount']").val(deductArray[oindex]);
                                             
                                $('#'+deductArray[oindex]+'-inactive-label').css('color','transparent');
                                 
                                 if(selectedDeduct.length>3){
                                    selectedDeduct = deductArray[oindex].substring(0,deductArray[oindex].length-3)+','+deductArray[oindex].substring(deductArray[oindex].length-3,deductArray[oindex].length);
                                 }
                                 
                                 $('#deduct-active-label').text('HKD$'+selectedDeduct);
                                 var activeLabelWidth = $('#deduct-active-label').width();
                                 
                                 if(oindex == 0){
                                    $('#coordinate-active').css('padding-left',0);
                                    $('#coordinate-active').css('text-align','left');
                                 } else if(oindex == size-1){
                                    $('#coordinate-active').css('padding-left',0);
                                    $('#coordinate-active').css('text-align','right');
                                 } else {
                                    $('#coordinate-active').css('padding-left',centerPosition+perProgressBarWidth*oindex-parseInt(activeLabelWidth/2));
                                    $('#coordinate-active').css('text-align','left');
                                 }
                                 
                                 $('#deduct-active-label').css('color','#39B54A');
                                                                                                           
                                 $("input[id*='Quote-PlusDeductibleAmount']").trigger('change');                            
                             </script>
                     </div>
