<apex:page >
    <apex:stylesheet value="{!URLFOR($Resource.Cigna_HK_Icons, 'Cigna-HK-Icons.css')}"/>
    <style>
        @media screen and (max-width: 767px) and (min-width: 0px) {
            .read-more-content {
                font-size: 0;
                display: none;
                transition: .25s ease;
            }
            .read-more-checkbox:checked ~ .read-more-wrapper .read-more-content {
                display: block;
                font-size: inherit;
            }
            .read-more-button {
                display: inline-block;
                padding: 5px;
                border-radius: 3px;
                font-size: .9em;
                margin-bottom: 15px !important;
                cursor: pointer;
            }
            .productDetails div{
                padding: 0px;
            }
            .productDetails{
                padding: 0px;
            }
        }
        @media screen and (min-width: 768px) {
            .read-more-content {
                display: block;
                font-size: inherit;
            }
            .read-more-button {
                display: none !important;
            }
        }
        .read-more-checkbox {
            display: none;
        }
        .read-more-checkbox ~ .read-more-button:before {
            content: '{!$Label.Show_more}';
        }
        .read-more-checkbox:checked ~ .read-more-button:before {
            content: '{!$Label.Show_less}';
        }
        
    </style>
    <div class="bs col-sm-6 col-xs-12 productDetails">
        <div class="bs col-sm-2 col-xs-2">
            <i class="ci-hk-plus ci-hk-2x"></i>
        </div>
        <div class="bs col-sm-10 col-xs-10">
            <!-- Rancho_HKITSFDC264_20250903 - update font -->
            <label>{!$Label.Coverage_Supplementary_Medical_Bene}</label>
                <p>{!$Label.Coverage_PM_Supplementary_Medical_Bene}</p>
        </div>
    </div>
    <!-- Link  22 - Chubb Transition - Product changes -->
