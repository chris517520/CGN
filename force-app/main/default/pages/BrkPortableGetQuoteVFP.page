                              <a class="bs cigna-fg-orange-medium onclick-pointer" id="quote-save-btn" style="text-decoration: none;" onClick="ServerQuote.saveQuote();"> 
                              <i aria-hidden="true" class="ci-hk-briefcase"></i>{!$Label.Save}
                              </a>
                            </h5>
                        </apex:outputPanel>
                        <div id='phone'>
                          <apex:outputPanel rendered="{!showSummary}" layout="none">
