                        console.log('End of next step');
                        
                    }   
                    
                    window.previousStep = function(){
                        var currentStepNum = $('.content-step:visible').data('step');
                        var previousStepNum = currentStepNum - 1;
                        
                        if (currentStepNum > 1){
                            <!-- 4 to 2 -->
                            if(currentStepNum == '4'){
