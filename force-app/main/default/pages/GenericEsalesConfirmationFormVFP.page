<apex:page docType="html-5.0" sidebar="false" showHeader="false" controller="GenericEsalesCtrl">
    <head>
        <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, viewport-fit=cover"/>
    </head>
    <apex:form id="confirmationfrm" >
        <apex:actionFunction action="{!nextStep}" name="nextStep" reRender="wrapper,wrapperApp,errmessage" />        
        <apex:outputPanel id="confirmation-section" rendered="{!currentStep == STEP_CONFIRMATION}" styleClass="bs row" layout="block">
        
        <!--Overall AUW Result Panel--> 
        <apex:outputPanel id="overall-result-panel">
            <div class="bs col-xs-12 form-horizontal">
               <div class="bs" style="padding-bottom: 20px;">
                   <p><h4 class="bs cigna-fg-blue-medium">{!$Label.Application_Result}</h4></p>                   
                   <!--Customer Portal Overall Result-->
                   <apex:outputPanel rendered="{!isCustomerPortal}">
                        <!--<apex:outputPanel rendered="{!personPH.AUW_Result__c == 'Accept' && txn.AUW_Overall_Result__c == 'Accept'}" layout="none">-->
                        <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Accept'}" layout="none">
                            <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_Exclusion}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_Decline}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note2}</p>
                            </apex:outputPanel>
                            <!--apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none"--> <!--commented SUQ Connor-->
                            <apex:outputPanel rendered="{!!hasDecline && !hasExclusion && !hasLoading}" layout="none"> <!--SUQ Connor-->
                                <p>{!$Label.eSales_AUW_result_Accept}</p>
                            </apex:outputPanel>
                            <!--SUQ Connor-->
                            <apex:outputPanel rendered="{!!hasDecline && !hasExclusion && hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_result_PH_refer}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept'))  && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <apex:outputPanel rendered="{!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_refer_with_Exclusion}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!hasExclusion}" layout="none">
                                <p>{!$Label.eSales_AUW_result_with_refer}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline || hasAccept}" layout="none">
                                <p>{!$Label.eSales_AUW_result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none">
                                <p>{!$Label.eSales_AUW_result_Note2}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>

                        <apex:outputPanel rendered="{! !(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <p>{!$Label.eSales_AUW_result_PH_refer}</p>
                        </apex:outputPanel>
                        <!--<apex:outputPanel rendered="{!personPH.AUW_Result__c == 'Decline'}" layout="none">-->
                        <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Decline'}" layout="none">
                            <p>{!$Label.eSales_AUW_result_PH_Decline}</p>
                        </apex:outputPanel>
                    </apex:outputPanel>
                    
                    <!--Broker Portal Overall Result-->
                    <apex:outputPanel rendered="{!!isCustomerPortal}">
                        <!--<apex:outputPanel rendered="{!(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Accept'}" layout="none">-->
                        <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Accept'}" layout="none">
                            <!--apex:outputPanel rendered="{!hasExclusion}" layout="none"--><!--commentted by application result change SUQ Connor 11/08/2021-->
                            <apex:outputPanel rendered="{! hasExclusion && !hasLoading}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_with_Exclusion}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 start-->
                            <apex:outputPanel rendered="{! hasExclusion && hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_with_Exclusion_Loading}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{! !hasExclusion && hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 end-->
                            <apex:outputPanel rendered="{!hasDecline}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_with_Decline}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusion || hasDecline}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Note1}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!hasExclusionC1 || hasExclusionC2 || hasExclusionC3}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!!hasDecline && !hasExclusion}" layout="none">
                                <p>{!$Label.eSales_Brk_AUW_Result_Accept}</p>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <!--<apex:outputPanel rendered="{!(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">-->
                        <apex:outputPanel rendered="{!(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Note2}</p>
                            <!--application result change SUQ Connor 11/08/2021 start-->
                            <apex:outputPanel rendered="{! hasLoading}" layout="none">
                                <p>{!$Label.eSales_AUW_Result_Accept_Loading}</p>
                            </apex:outputPanel>
                            <!--application result change SUQ Connor 11/08/2021 end-->
                            <br/>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                        </apex:outputPanel>
                        <!--<apex:outputPanel rendered="{!(personPH.AUW_Result__c  == 'Referred' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Referred'))}" layout="none">-->
                        <apex:outputPanel rendered="{! !(personPH.AUW_Result__c == 'Accept' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Accept')) && txn.AUW_Overall_Result__c == 'Referred'}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer1}</p>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer2}</p>
                            <br/>
                            <p>{!$Label.eSales_Brk_AUW_Result_Refer3}</p>
                        </apex:outputPanel>

                        <!--<apex:outputPanel rendered="{!(personPH.AUW_Result__c  == 'Decline' || (!personPH.Is_Person_Insured__c && personSP.AUW_Result__c == 'Decline'))}" layout="none">-->
                        <apex:outputPanel rendered="{!txn.AUW_Overall_Result__c == 'Decline'}" layout="none">
                            <p>{!$Label.eSales_Brk_AUW_Result_Decline}</p>
                        </apex:outputPanel>
                    </apex:outputPanel>
               </div>
            </div>
        </apex:outputPanel>
        
        <!--Personal Information Panel--> 
        <!-- Add by Manuel to use list for sequence show -->
        <apex:outputPanel id="personnal-information-panel" > 
            <apex:repeat value="{!lsKeyPI}" var="key">
                <div class="bs col-xs-12 form-horizontal">
                    <div class="bs" style="padding-bottom: 20px;">
                        <h4 class="bs cigna-fg-blue-medium">{!IF(key==ESALES_PI_KEY_PH,IF(personInsuredMap[key].Is_Person_Insured__c, $Label.Policy_Holder_s_Application, $Label.Policy_Holder_s_Information),
                                                                IF(key==ESALES_PI_KEY_SP,$Label.Esales_Adult_2_s_Application,
                                                                    IF(key==ESALES_PI_KEY_CH1,$Label.Child_1_s_Application,
                                                                        IF(key==ESALES_PI_KEY_CH2,$Label.Child_2_s_Application,$Label.Child_3_s_Application))))}
                        </h4>
                      
                        <apex:outputPanel id="personal-auw-result" styleClass="bs form-group" layout="block" rendered="{!personInsuredMap[key].Is_Person_Insured__C}">
                            <div class="bs col-sm-3">
                                <apex:outputLabel value="{!$Label.Application_Result}" for="Confirm-Application-Result"/>
                            </div>
                            <div class="bs col-sm-9">
                                <!--apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c==null}" layout="none">
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c!=null}" layout="none">
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept_With_Condition}</p>
                                </apex:outputPanel--><!--commentted by Connor SUQ  09/08/2021-->
                                <!--ph application result change 09/08/2021 Connor SUQ start-->
                                <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c==null && (personInsuredMap[key].Loading_Rate__c==null || personInsuredMap[key].Loading_Rate__c==0)}" layout="none">  
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c!=null && (personInsuredMap[key].Loading_Rate__c==null || personInsuredMap[key].Loading_Rate__c==0)}" layout="none">
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept_With_Condition}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{! isCustomerPortal && personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c==null && personInsuredMap[key].Loading_Rate__c!=null && personInsuredMap[key].Loading_Rate__c>0}" layout="none"> 
                                    <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i>{!$Label.AUW_Overall_Refer}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{! !isCustomerPortal && personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c==null && personInsuredMap[key].Loading_Rate__c!=null && personInsuredMap[key].Loading_Rate__c>0}" layout="none"> 
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i>{!$Label.AUW_Overall_Accept_With_Loading}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{! isCustomerPortal && personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c!=null && personInsuredMap[key].Loading_Rate__c!=null && personInsuredMap[key].Loading_Rate__c>0}" layout="none"> 
                                    <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i> {!$Label.AUW_Overall_Refer}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{! !isCustomerPortal && personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c!=null && personInsuredMap[key].Loading_Rate__c!=null && personInsuredMap[key].Loading_Rate__c>0}" layout="none"> 
                                    <p><i class="bs cigna-fg-green-medium fa fa-check-circle fg-lg"></i> {!$Label.AUW_Overall_Accept_With_2_Conditions}</p>
                                </apex:outputPanel>
                                <!--ph application result change 09/08/2021 Connor SUQ end-->
                                <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Referred'}" layout="none">
                                    <p><i class="ci-hk-clipboard" style="color: #E35205 !important;"></i> {!$Label.AUW_Overall_Refer}</p>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Decline'}" layout="none">
                                    <p><i class="bs cigna-fg-red-alert fa fa-times-circle fg-lg"></i> {!$Label.AUW_Overall_Decline}</p>
                                </apex:outputPanel>
                            </div>
                            
                            <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Accept' && personInsuredMap[key].Exclusion__c!=null}" layout="none">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_EXCLUSION]}" for="Confirm-Application-Exclusion-Reason"/>
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputPanel rendered="{!personInsuredMap[key].Exclusion__c!=null || personInsuredMap[key].Chinese_Exclusion__c!=null}" layout="none">
                                        <p>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_EXCLUSION_NOTE1]}</p>
                                        <apex:outputPanel rendered="{!$Label.Header_Language_Code == 'en_US'}">
                                            <!--p>{!personInsuredMap[key].Chinese_Exclusion__c}</p--><!--commented by Connor for issue VU2-109-->
                                            <apex:repeat value="{!exclusionsCH[key]}" var="reasons">
                                            <p>{!reasons}</p>
                                            </apex:repeat>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!$Label.Header_Language_Code == 'zh_TW'}">
                                            <!--p>{!personInsuredMap[key].Exclusion__c}</p--><!--commented by Connor for issue VU2-109-->
                                            <apex:repeat value="{!exclusionsEN[key]}" var="reasons"> 
                                            <p>{!reasons}</p>
                                            </apex:repeat>
                                        </apex:outputPanel>
                                        <p>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_EXCLUSION_NOTE2]}</p>
                                    </apex:outputPanel>
                                </div>
                            </apex:outputPanel>
                            
                            <apex:outputPanel rendered="{!personInsuredMap[key].AUW_Result__c == 'Decline' && personInsuredMap[key].Decline_Reason__c!=null}" layout="none">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_DECLINE]}" for="Confirm-Application-Exclusion-Reason"/>
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputPanel rendered="{!personInsuredMap[key].Decline_Reason__c!=null}" layout="none">
                                        <p>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_DECLINE_NOTE]}</p>
                                        <!--p>{!personInsuredMap[key].Decline_Reason__c}</p--> <!--commented by Connor for issue VU2-109-->
                                        <apex:repeat value="{!declineReasons[key]}" var="reasons">
                                        <p>{!reasons}</p>
                                        </apex:repeat> 
                                    </apex:outputPanel>
                                </div>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        
                        <apex:outputPanel id="personal-basic-information-panel" >
                             <div class="bs form-group">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_GIVEN_NAME]}" />
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputText value="{!personInsuredMap[key].Given_Name__c}"/>
                                </div>
                            </div>
                            <div class="bs form-group">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_FAMILY_NAME]}" />
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputText value="{!personInsuredMap[key].Family_Name__c}"/>
                                </div>
                            </div>
                            <div class="bs form-group">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_PERSONAL_ID]}" />
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputText value="{!personInsuredMap[key].Personal_ID__c}"/>
                                </div>
                            </div>
                            <div class="bs form-group">
                                <div class="bs col-sm-3">
                                    <apex:outputLabel value="{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_DATE_OF_BIRTH]}" />
                                </div>
                                <div class="bs col-sm-9">
                                    <apex:outputText id="Confirm-PH-DOB" value="{0, date, yyyy/MM/dd}">
                                        <apex:param value="{!personInsuredMap[key].Date_of_Birth__c}" />
                                    </apex:outputText>
                                </div>
                            </div>
                        </apex:outputPanel>
                        
                        <apex:outputPanel id="diffrent-plan-level-pannel" rendered="{!txn.Different_Plan_Level__c}">
                            <apex:outputPanel rendered="{!personInsuredMap[key].Question_1__c!=null}">
                                <div class="bs form-group">
                                    <div class="bs col-sm-3">
                                        <label>{!planLevelKeyWordTranslationMap[personInsuredMap[key].Question_1__c]}</label>
                                    </div>
                                    <div class="bs col-sm-9">
                                        {!planLevelInfoTranslationMap[personInsuredMap[key].Answer_1__c]}
                                    </div>
                                </div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!personInsuredMap[key].Question_2__c!=null}">
                                <div class="bs form-group">
                                    <div class="bs col-sm-3">
                                        <label>{!planLevelKeyWordTranslationMap[personInsuredMap[key].Question_2__c]}</label>
                                    </div>
                                    <div class="bs col-sm-9">
                                         {!planLevelInfoTranslationMap[personInsuredMap[key].Answer_2__c]}
                                    </div>
                                </div>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <!--added quoted price section 09/08/2021 Connor SUQ-->
                        <c:QuotedPriceSection monthlyQuote="{!personInsuredMap[key].Original_Amount_Monthly__c}" annualQuote="{!personInsuredMap[key].Original_Amount_Annually__c}" medicalLoadingStr="{! IF(personInsuredMap[key].Loading_Rate__c==null||personInsuredMap[key].Loading_Rate__c==0,'0',TEXT(personInsuredMap[key].Loading_Rate__c*100))}" loadingReason="{!personInsuredMap[key].Decline_Reason__c}" totalPremiumMonthly="{!personInsuredMap[key].Quote_Amount_Monthly__c} " totalPremiumYearly="{!personInsuredMap[key].Quote_Amount_Annually__c}" auwResult="{!personInsuredMap[key].AUW_Result__c}" isCustomerPortal="{!isCustomerPortal}" hasLoading="{! IF(personInsuredMap[key].Loading_Rate__c!=null&&personInsuredMap[key].Loading_Rate__c>0,true,false)}"/>
                    </div>
                </div>   
            </apex:repeat>
        </apex:outputPanel>
        
        <!--Same plan level Information Panel--> 
        <apex:outputPanel id="same-plan-level-panel" rendered="{!!txn.Different_Plan_Level__c}">
            <div class="bs col-xs-12 form-horizontal">
                <div class="bs col-xs-12"><hr /></div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!$Label.Selected_Benefit}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!product.Name__c}
                    </div>
                </div>
                
                <!-- Add by Manuel -->
                <apex:outputPanel rendered="{!personPH.Question_1__c!=null}">
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <label>{!planLevelKeyWordTranslationMap[personPH.Question_1__c]}</label>
                        </div>
                        <div class="bs col-sm-9">
                            {!planLevelInfoTranslationMap[personPH.Answer_1__c]}
                        </div>
                    </div>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!personPH.Question_2__c!=null}">
                    <div class="bs form-group">
                        <div class="bs col-sm-3">
                            <label>{!planLevelKeyWordTranslationMap[personPH.Question_2__c]}</label>
                        </div>
                        <div class="bs col-sm-9">
                            {!planLevelInfoTranslationMap[personPH.Answer_2__c]}
                        </div>
                    </div>
                </apex:outputPanel>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_CONTACT_NO]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Contact_No__c}
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_ADDRESS_LINE1]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Address_Line_1__c}
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_ADDRESS_LINE2]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Address_Line_2__c}
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_ADDRESS_LINE3]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Address_Line_3__c}
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_ADDRESS_LINE4]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Address_Line_4__c}
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <label>{!PersonInfoKeyWordTranslationMap[ESALES_LABEL_EMAIL_ADDRESS]}</label>
                    </div>
                    <div class="bs col-sm-9">
                        {!personPH.Email_Address__c}
                    </div>
                </div>
            </div>
        </apex:outputPanel>
        
        <!-- GL31 jack add -->
        <apex:outputPanel rendered="{!isBrokerPortal}">
            <div class="bs col-xs-12 form-horizontal">
                <div class="bs col-xs-12">
                    <hr />
                </div>
                <!-- Q1 part -->
                <div class="bs form-group">
                    <div class="bs col-sm-9">
                        <apex:outputLabel style="color: #188CCC" value="Q1. {!transField['Assessment_Q_1'][$Label.Header_Language_Code_Current]}"
                        />
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <apex:outputLabel value="{!transField['Q1A1'][$Label.Header_Language_Code_Current]} / {!transField['Hospitalization'][$Label.Header_Language_Code_Current]}"
                            for="Q1A1" />
                    </div>
                    <div class="bs col-sm-9">
                        <apex:outputText id="Q1A1" value="{!assessmentResult.Q1A1}" />
                    </div>
                </div>
                <div class="bs form-group">
                    <div class="bs col-sm-3">
                        <apex:outputLabel value="{!transField['Q1A2'][$Label.Header_Language_Code_Current]} / {!transField['Critical_Illness'][$Label.Header_Language_Code_Current]}"
                            for="Q1A2" />
                    </div>
                    <div class="bs col-sm-9">
                        <apex:outputText id="Q1A2" value="{!assessmentResult.Q1A2}" />
