<apex:page showHeader="false" applyHtmlTag="false" renderAs="PDF" cache="true"  controller="BrkQuotationCtrl"> 
    <apex:stylesheet value="{!URLFOR($Resource.Cigna_HK_Icons, 'Cigna-HK-Icons.css')}"/>
    <head>
        <style>
            
            @media print
            {
            table {page-break-inside:auto }
             tr    { page-break-inside:avoid; page-break-after:auto }
             }
            
            @page:first {
                margin: -20px -10px 100px -10px;
                @bottom-left {
                    content:element(footer_first);
                }
                @bottom-right {
                    content:element(logo_first);
                }
            }
            
            @page {
                margin: 80px 60px 120px 60px;
                
                @bottom-left {
                    content:element(footer);
                }
                @bottom-right {
                    content:element(logo);
                }
            }
            
            .Content{
                width:680px;
            }
            
            div.footer{
                position: running(footer);
            }
            
            div.logo{
                position: running(logo);
            }
            
            div.footer_first{
                position: running(footer_first);
            }
            
            div.logo_first{
                position: running(logo_first);
            }
            
            body {}
            
            div, table, tr, td{
                font-family: "Arial Unicode MS", sans-serif;
            }
            
            .HeadText{
                font-weight: bold;
                font-size:22pt;
            }
            .SHeadText{
                font-weight:bold;
                font-size:20pt;
            }
            .BodyText{
                weight:720px;
            }
            .THeadText{
                font-weight:bold;
                color:#008B00;
                font-size:18pt;
            }
            .SBodyText{
                font-weight:bold;
                font-size:15pt;
            }
            
            .small-text{
                font-size:10pt;
            }
            
            .back{
                height:960px;
                background-attachment: fixed;
                background-image: url({!URLFOR($Resource.BokerPortal)});
                <!--background-color: yellow;-->
            }
            .table-title{
                font-size:18pt;
                background-color: #188CCC !important;
                color: #fff !important;
                border: 1px;
                border-style: solid;
                border-color: #fff;
                font-weight: bold;
            }
            .table-subtitle{
                font-size:12pt;
                font-weight: bold;
                background-color: #B1DDF5 !important;
                border: 1px;
                border-style: solid;
                border-color: #fff;
            }
            .table-content{
                font-size:12pt;
                border: 1px;
                border-style: solid;
                border-color: #fff;
            }
            .table-content > tr {
                background-color: #DAEFFA;
            }
            .table-highlight{
                background-color: white;
                border: 3px;
                border-style: solid;
                border-color: white;
            }
            
            .table-process{
                border-color: #188CCC;
                border: 1px;
                border-style: solid;
                vertical-align: text-top;
            }
            

            .cigna-fg-orange-medium {
                color: #F68621 !important;
            }
            .cigna-fg-orange-dark {
                color: #E35205 !important;
            }
            .cigna-fg-white {
                color: #fff !important;
            }
            .cigna-fg-blue-medium {
                color: #188CCC !important;
            }
            .cigna-fg-blue-dark {
                color: #004986 !important;
            }
            .cigna-bg-green-dark {
                background-color: #007A3E !important;
            }
            .cigna-bg-blue-medium {
                background-color: #188CCC !important;
            }
            .cigna-bg-blue-dark {
                background-color: #004986 !important;
            }
            .justify{
                text-align:justify;
            }
        </style>
    </head>

    <div class="footer" ><!-- style="margin:20px 0px 20px -40px;" -->
         <div style="width:145px;float:left;bottom:20px;left:0px;margin-top:55px;margin-bottom:10px; margin-left: -40px;" class="together">
             <apex:image url="{!$Resource.Together_CH}" width="145px" /> 
        </div>    
    </div>
    
    <div class="logo"><!--  style="margin:15px -25px 20px 0px;padding-bottom:30px;" -->
       <div style="width:145px; float:right;bottom:20px;right:10px;margin-top:20px;margin-bottom:10px;margin-right:-25px;padding-bottom: 10px;">
           <apex:image url="{!$Resource.TreeCigna_CH}" width="145px" /> 
       </div>    
    </div>
    <div class="footer_first" style ="margin:20px 0px 0px 30px;">
         <div style="width:80px;float:left;top:70px;left:0px; " class="together">
             <apex:image url="{!$Resource.Together_CH}" width="80px" /> 
        </div>    
    </div>
    
    <div class="logo_first" style="margin:0px 45px 60px 0px;padding-bottom:30px">
       <div style="width:145px; float:right;top:30px;right:10px">
           <apex:image url="{!$Resource.TreeCigna_CH}" width="145px" /> 
       </div>    
    </div>
   
    <div class="back">
        <div style="padding-left:20px">
             <table style="border-style: none;width:700px;table-layout: fixed; position: absolute;top:700px;right:0px;left:10px;bottom:0px;font-size:26pt;background-color:#FFFAFA" border="1" cellspacing="0" cellpadding="15"  >
                <tr class="cigna-bg-blue-medium cigna-fg-white">
                    <th style="border-style: none;font-size:24pt;font-weight:bold;line-height:35px;padding: 20px;padding-left:40px;">
                        為您而設的全面醫療保險計劃
                    </th>
                </tr> 
                <tr class="cigna-bg-blue-dark cigna-fg-white">
                    <th style="border-style: none;font-size:24pt;font-weight:bold;padding:20px;padding-left:40px">               
                        致<apex:outputText value="{!Name}"/>的建議書
                    </th>
                </tr>
            </table>
        </div>
    </div>
    
    <div class="Content">
    <div class="cigna-fg-blue-dark HeadText" style="page-break-before: always;"  >
        <apex:outputText >
            為何選擇信諾？
        </apex:outputText>
    </div><br/>
    <div class="cigna-fg-blue-medium SHeadText">
        <apex:outputText >
            健康及保健服務 引領全球
        </apex:outputText>
    </div><br/>
    <div class="BodyText justify">
        <apex:outputText >
            身為環球健康保障專家，信諾集團致力幫助客戶提升健康、安康及提供安心的保障。信諾的前身公
            司自1792年提供專業保健服務。現時，信諾繼續不斷創新、發展及增長其環球業務。我們的業務遍
            佈30多個國家及司法管轄區，積極照顧全球超過1.8億客戶的健康。
        </apex:outputText>
    </div><br/>
    <div class="cigna-fg-orange-medium THeadText" >
        <apex:outputText >
            關於信諾集團
        </apex:outputText>
    </div>
    
    <table cellspacing="0" cellpadding="20" style="width:100%;table-layout:fixed" align="justify">
        <tr class="small-text" style="vertical-align:text-top;text-align:center">
            <th>
                <apex:image value="{!$Resource.certificate}" width="80px" style="text-align:center"/><br/>
                全球超過<br/>
                70,000名員工          
            </th>
            <th>
                 <apex:image value="{!$Resource.connection}" width="80px" /><br/>
                與全球約1.8億客戶<br/>
                建立了業務關係
            </th>
            <th>
                <apex:image value="{!$Resource.badge}" width="80px"/><br/>
                2020年《財富》<br/>
                500強排名13位
            </th>
        </tr>
        <tr class="small-text" style="text-align:center;vertical-align:text-top;">
            <th>
                <apex:image value="{!$Resource.tellurion}"   width="80px" style="text-align:center"/><br/>
                業務遍佈超過<br/>
                30個國家及地區
            </th>
            <th>
                <apex:image value="{!$Resource.increase}"   width="80px" style="text-align:center"/><br/>
                調整後總收入達<br/>
                1400億美元
            </th>
            <th>
               <apex:image id="theImage" value="{!$Resource.Doctor}"   width="80px" style="text-align:center"/><br/>
                環球網絡擁有超過150萬的<br/>
                專業醫護人員、診所及醫療機構
            </th>
        </tr>
    </table>
    <div style="width:680px;font-size:8pt">
        <apex:outputText >
            以上數據僅供參考用途，並不針對信諾集團旗下個別營運附屬公司。<br/>
            數據是基於信諾集團截至2019年12月31日的內部資料，並有可能作出更改。
        </apex:outputText>
    </div><br/>

    <div class="cigna-fg-orange-medium THeadText">
        <apex:outputText >
            關於信諾香港
        </apex:outputText>
    </div><br/>
    <div class="BodyText justify">
        <apex:outputText >
            信諾自1933年投入香港保險業務，一直以來策劃無微不至的保障，為客戶的不同人生階段提供適
            切建議，以幫助客戶提升健康、安康及提供安心的保障。
        </apex:outputText>
    </div>
    <div class="cigna-fg-blue-dark HeadText" style="page-break-before: always;"  >
        <apex:outputText >
            為您而設的建議書摘要
        </apex:outputText>
    </div><br/>
    <table class="table-content" border="1" cellspacing="0" cellpadding="10" style="width:100%;table-layout: fixed;border-top:none;border-bottom:none;">
        <tr class="table-title">
            <td border="1" style="border-style: solid;border-color: #fff;" colspan="4">
                <apex:outputText >產品：<apex:outputText value="{!ProductName}"/></apex:outputText>
            </td>
        </tr>
    </table>
    <table class="table-content" border="1" cellspacing="0" cellpadding="10" style="width:100%;table-layout: fixed;border-top:none;border-bottom:none;{!NumberAdult}">
        
        <tr class="table-subtitle">
            <th border="1" style="border-style: solid;border-color: #fff;" colspan="4">
                保障摘要（<apex:outputText value="{!perRole1}"/>）
            </th>
        </tr>
        <tr>
            <td border="1" style="border-style: solid;border-color: #fff;">
                年齡：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!AgeAdult1}"/>
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
               性別：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                 <apex:outputText value="{!Gender1}"/>
            </td>
        </tr>
        <tr style="{!if(hasSmoHabit,'','display:none;')}">
            <td border="1" style="border-style: solid;border-color: #fff;">
                吸煙習慣：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!SmokerStatus}"/>
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                保單持有人
                （是／否）：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!Holder}"/>
            </td>
        </tr>
        <tr style="{!if(hasSmoHabit,'display:none;','')}">
            <td border="1" style="border-style: solid;border-color: #fff;">
                保單持有人
                （是／否）：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;" colspan="3">
                <apex:outputText value="{!Holder}"/>
            </td>
        </tr>
        <tr style="{!if(isElite,'','display:none;')}">
            <td border="1" style="border-style: solid;border-color: #fff;">
                年度墊底費：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!if(DifferentPlan == false,Deductible,if(es.Is_Adult_1_the_Policy_Holder__c,PH,PI))}"/>
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                受保地區：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!if(DifferentPlan == false,AreaCoverage,if(es.Is_Adult_1_the_Policy_Holder__c,AreaPH,AreaPI))}"/>
            </td>
        </tr>
        <tr style="{!if(isChoice,'','display:none;')}">
                <td border="1" style="border-style: solid;border-color: #fff;">
                    計劃級別:
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;" colspan="3">
                    <apex:outputText value="{!planLevel}"/>
                </td>           
            </tr>
            <tr style="{!if(isPlus || isVHIS,'','display:none;')}">
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        計劃級別:
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        <apex:outputText value="{!planLevelPH}"/>
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        墊底費:
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                         <apex:outputText value="{!IF(isVHIS,PH,Deductible)}"/>
                    </td>
            </tr>
            <tr style="{!if(isSCI,'','display:none;')}">
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        計劃級別:
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        <apex:outputText value="{!planLevel}"/>
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        保障級別:
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;">
                         <apex:outputText value="{!levelofCov}"/>
                    </td>
            </tr>
            <tr style="{!if(isDMB,'','display:none;')}">
                    <td border="1" style="border-style: solid;border-color: #fff;">
                        計劃級別:
                    </td>
                    <td border="1" style="border-style: solid;border-color: #fff;" colspan="3">
                        <apex:outputText value="{!planlevelPH}"/>
                    </td>
            </tr>
            <tr style="{!if(isPBT || isPBC,'','display:none;')}">
                <td border="1" style="border-style: solid;border-color: #fff;">
                    計劃級別:
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    <apex:outputText value="{!planLevelPH}"/>
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    賠償比率:
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                     <apex:outputText value="{!reimbursement1}"/>
                </td>
            </tr>
        <tr>
            <td border="1" style="border-style: solid;border-color: #fff;">
                保單貨幣：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                港幣
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                每年最高賠償額：
            </td>
            <td border="1" style="border-style: solid;border-color: #fff;">
                <apex:outputText value="{!IF(BLANKVALUE(annualLimitPH, 0) > 0, '{0, number, ###,###,###,##0}', '-')}" >
                       <apex:param value="{!annualLimitPH}"/>
                </apex:outputText>
            </td>
        </tr>
        <tr>
            <td  rowspan="{!ProductAdult}" border="1" style="border-style: solid;border-color: #fff;">
               自選保障：
            </td>
            <td colspan="3" border="1" style="border-style: solid;border-color: #fff;{!if(ProductAdult>1,'display:none;','')}">
                {!NA}  
            </td>
        </tr>
        <apex:repeat value="{!RiderListPH}" var="rider">
           <tr>
              <td colspan="3" border="1" style="border-style: solid;border-color: #fff;">              
                  <apex:outputText value="{!rider.name}"/>
                  &nbsp;-&nbsp;
                  <apex:outputText value="{!rider.cur}"/>
                  &nbsp;（每年）&nbsp;
                  <apex:outputText value="{!IF(BLANKVALUE(rider.amountA, 0) > 0, '{0, number, ###,###,###,##0.00}', '-')}" >
                     <apex:param value="{!rider.amountA}"/>
                  </apex:outputText>
                  ,&nbsp;（每月）&nbsp;
                  <apex:outputText value="{!IF(BLANKVALUE(rider.amountM, 0) > 0, '{0, number, ###,###,###,##0.00}', '-')}" >
                     <apex:param value="{!rider.amountM}"/>
                  </apex:outputText>                                   
              </td>
            </tr>
        </apex:repeat>
    </table>
    <table class="table-content" border="1" cellspacing="0" cellpadding="10" style="width:100%;table-layout: fixed;border-top:none;border-bottom:none;{!NumberAdult}">
            <tr style="font-weight:bold;">
                <td border="1" style="border-style: solid;border-color: #fff;">
                    保費：
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    &nbsp;（每年）港幣&nbsp;
                    <apex:outputText value="{!IF(BLANKVALUE(QuoteAmountAnnuallyPH, 0) > 0, '{0, number, ###,###,###,##0.00}', '-')}" >
                    <apex:param value="{!QuoteAmountAnnuallyPH}" />
                    </apex:outputText>
                </td>
                <td colspan="2" border="1" style="border-style: solid;border-color: #fff;">
                    &nbsp;（每月）港幣&nbsp;
                    <apex:outputText value="{!IF(BLANKVALUE(QuoteAmountMonthlyPH, 0) > 0, '{0, number, ###,###,###,##0.00}', '-')}" >
                    <apex:param value="{!QuoteAmountMonthlyPH}"/>
                    </apex:outputText>
                </td>
            </tr>
        </table>
        <table class="table-content" border="1" cellspacing="0" cellpadding="10" style="width:100%;table-layout: fixed;border-top:none;border-bottom:none;{!NumberAdult2}">
            <tr class="table-subtitle">
                <th colspan="4" style="border-style: solid;border-color: #fff;">
                    保障摘要（<apex:outputText value="{!perRole2}"/>）
                </th>
            </tr>
            <tr>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    年齡：
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    <apex:outputText value="{!AgeAdult2}"/>
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    性別：
                </td>
                <td border="1" style="border-style: solid;border-color: #fff;">
                    <apex:outputText value="{!Gender2}"/>
                </td>
            </tr>
            <tr style="{!if(hasSmoHabit,'','display:none;')}">
