            </div>
        </apex:outputPanel>
        <!-- End of back button -->

        <!-- 20200902 - minghua - Check Sensitivity code for e-sales -->
        <apex:outputPanel id="errmessage">
            <div class="bs row">
                <apex:pagemessages ></apex:pagemessages>
            </div>
        </apex:outputPanel>
