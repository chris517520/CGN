<apex:page docType="html-5.0" id="TMReportCenterVFP" controller="TMReportCenterCtrl" showHeader="false" title="Report Center">
    <script type="text/javascript" src="{!URLFOR($Resource.DD_Demo, 'js/jquery.min.js')}"></script>
    <script>
        function viewDetails(url) {
            if (sforce.console.isInConsole()) {
                var rnd = Math.floor(Math.random() * 1000000000000);               
                sforce.console.openPrimaryTab(null, url, true, 'Application Details');
                sforce.console.setCustomConsoleComponentVisible(false);
            } else {
                window.open(url, '_blank');
            }
        }
    </script>
    <apex:includeScript value="/support/console/41.0/integration.js"/>
    <apex:pageBlock id="displayReportList" title="{!$Label.TM_Report_Center}" >
      <apex:repeat value="{!reportMap}" var="report" id="report">
        <p onclick="viewDetails('{!report.URL}');" style="cursor: pointer;">
            <apex:outputtext value="{0, number, ##} " style="display: block;width: 25px;float: left;">
                <apex:param value="{!report.data.SN__c}"></apex:param>
            </apex:outputtext>
            &nbsp; 
            <apex:outputText value="{!report.data.Report_Name__c}" id="reportLabel" style="display: block;float: left;"></apex:outputText>
        </p>
      </apex:repeat>
    </apex:pageBlock>
</apex:page>