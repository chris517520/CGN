<apex:page sidebar="false" controller="CSGroupPolicyCorrespondencesCtrl">
    <script type="text/javascript" src="{!URLFOR($Resource.DD_Demo, 'js/jquery.min.js')}"></script>
    <apex:includeScript value="/support/console/33.0/integration.js"/>
    <script>
        //sforce.console.setTabTitle("{!targetCertNo}");
    </script>
    <apex:form id="newFaxForm">
        <apex:pageBlock title="{!$Label.efulfillment_policy_correspondences}"> 
            <apex:outputPanel rendered="{!curGrpCorDocList.size>0}" layout="block" id="corDocCtn">
                <apex:pageblockTable id="corDocTable" value="{!curGrpCorDocList}" var="myList" >                        
                    <apex:column headerValue="Document Name" >
                        <apex:commandLink action="{!getDoc2}" target="_blank">
                             {!myList.Description}
                            <apex:param assignTo="{!targetAttachmentKey}" id="targetAttachmentKey" name="targetAttachmentKey" value="{!myList.attachmentKey}" />
                        </apex:commandLink>
                    </apex:column>
                    <apex:column headerValue="Date">
                        {!myList.createdDateStrDisp}
                    </apex:column>                   
                </apex:pageblockTable>                            
            </apex:outputPanel>
            <apex:outputPanel layout="block" id="noDataMsg" >
                <apex:outputLabel >No document found.</apex:outputLabel>
            </apex:outputPanel>
        </apex:pageBlock>
    </apex:form>
</apex:page>