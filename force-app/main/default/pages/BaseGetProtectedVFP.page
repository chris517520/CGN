<!--Please change apex page title-->
<apex:page docType="html-5.0" sidebar="false" cache="false" showHeader="false" title="Get Protected" controller="GetProtectedCtrl">
    <head>
        <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, viewport-fit=cover"/>
        <style>
            <!--additional inline css-->
            .sfdc_richtext li {
                margin-bottom: 5px;
            }
            .bs .cigna-belly-button-wrapper {
                top: -85px;
                position: absolute;
                left: 45%;
                background: #fff;
                border-radius: 100%;
            }
            .bs .cigna-belly-button {
                font-size: 30px !important;
                -webkit-border-radius: 1100%;
                -moz-border-radius: 100%;
                -o-border-radius: 100%;
                border-radius: 100%;
                border: 3px solid #39B54A;
                text-align: center;
                display: table-cell !important;
                vertical-align: middle;
                width: 60px;
                height: 60px;
            }
            .bs .cigna-content-area {
                padding: 20px;
            }
            .bs .cigna-btn-desktop {
                width: 30%;
                //text-transform: uppercase;
                /* Rancho_HKITSFDC264_20250903 - replace "Gotham SSm A", "Gotham SSm B" */
                font-family: "ValueSansRegular", "Microsoft Jhenghei", "LiHei Pro", "Heiti TC", Arial, Helvetica, sans-serif;
                font-style: normal;
                font-weight: 500;
            }
            .bs .cigna-btn-desktop-no-width {
                //text-transform: uppercase;
                /* Rancho_HKITSFDC264_20250903 - replace "Gotham SSm A", "Gotham SSm B" */
                font-family: "ValueSansRegular", "Microsoft Jhenghei", "LiHei Pro", "Heiti TC", Arial, Helvetica, sans-serif;
                font-style: normal;
                font-weight: 500;
            }
            .bs .cigna-list-group {
                margin-top: 20px;
                margin-bottom: 20px;
            }
            .bs .cigna-list-group-item {
                position: relative;
                display: block;
                padding: 20px 30px 20px 15px;
                background-color: #fff;
                border-bottom: 1px solid #ddd;
                color: #58595B;
            }
            .bs .cigna-list-group-item:hover {
                text-decoration: none;
                color: #58595B;
            }
            .bs .cigna-list-group-item:last-child {
                border: 0;
            }
            .bs .cigna-list-group-item i.item-right-arrow {
                position: absolute;
                right: 15px;
                top: 40%;
            }
            .bs .cigna-list-group-item div.item-right-arrow {
                position: absolute;
                right: 0px;
                top: 40%;
            }
            .bs .cigna-list-group-item > div {
                position: relative;
            }
            .bs .cigna-list-group-item { cursor: pointer; }
            @media (min-width: 768px) {
                .bs .cigna-div-table {
                    display: table !important;
                    table-layout: fixed;
                    width: 100%;
                    height: 100%;
                }
                .bs .cigna-div-table-left {
                    display: table-cell;
                    height: 100%;
                    float: none;
                    vertical-align: middle;
                    text-align: center;
                    border-right: 1px #D9D9D6 solid;
                }
                .bs .cigna-div-table-right {
                    display: table-cell;
                    height: 100%;
                    float: none;
                }
            }
            .bs .display-block {
                display: block !important;
            }
            .sfdc_richtext {
                /* Rancho_HKITSFDC264_20250903 - replace "Gotham SSm A", "Gotham SSm B" */
                font-family: "ValueSansRegular", "Microsoft Jhenghei", "LiHei Pro", "Heiti TC", Arial, Helvetica, sans-serif !important;
            }
            
        </style>
        <script>
            // Rancho_JR1898_20210325
            window.onload = function(){
                var selectedCode = $('#current-product-code').val();
                console.log('selectedCode:'+selectedCode);
                if(selectedCode =='PPC'){
                    $('#product-button-customer').hide();
                    $('#product-button-customer-tup').hide();
                    $('#product-button-customer-portable').hide();
                }
            }

            <!--additional inline js-->
            function preventDefault(event){
                if(event.preventDefault){
                    event.preventDefault();
                }
                else{
                        window.event.returnValue=false;
                        return false;

                }
            }

            var Cigna = Cigna || {};
            Cigna.GetProtected = function(){
                function selectCategory(evt) {
                    $('#product-filter-wrapper').fadeOut();
                    $('#product-back').fadeOut();
                    $('#content-placeholder-area').fadeIn();
                    
                    $('#current-step').val('product-selection');
                    $('#current-product-code').val('');
                    $('#cigna-selected-category').text($(this).text().toLowerCase());
                    //JOBR127-Remove Product in Customer Portal-20220524-Kirk
                    var category = '{!$Label.eSales_Medical_Health}';
                    $('#current-select-Category').val(category);
                    ServerGetProtected.loadProductList(category);
                    
                    evt.preventDefault();
                }
                
                function selectProduct() {
                    //Rancho_Combo_20201204
                    if('CBO' == $(this).data('product-code') && !Cigna.Util.isBrokerPortal()) {
                        var comboLink = document.getElementById('website_link');
                        comboLink.click();
                        return;
                    }

                    $('#product-selection-wrapper').fadeOut();
                    $('#product-back').fadeOut();
                    $('#content-placeholder-area').fadeIn();
                    
                    $('#current-step').val('product-info');
                    $('#current-product-code').val($(this).data('product-code'));
                    ServerGetProtected.loadProduct($(this).data('product-code'));

                    if (!Cigna.Util.isBrokerPortal()){
                        var prdname = $(this).data('product-name');
                        var lang='{!userLanguage}';
                        if (lang.substring(0,2)=='en')
                            lang = 'en_us';
                        GTMProductClickPush(prdname,lang);
                    }
                    //IBM JING PORTABLE CR 0423 START
                    var selectedCode = $('#current-product-code').val();
                    
                    /*if ((selectedCode=='PBT' && {!isTopUpElig}) 
                        || (selectedCode=='PBC' && {!isCoversionElig})
                        || (selectedCode !='PBT' && selectedCode !='PBC' )) 
                    {
                        $('#product-button-customer').show();
                    
                    }else
                    {
                         $('#product-button-customer').hide();
                    }*/
                    //IBM JING PORTABLE CR 0423 END
                    // Modified by IBM(Roy) due to PEI-98 START
                    if ((selectedCode=='PBT' && {!isTopUpElig}) 
                        || (selectedCode=='PBC' && {!isCoversionElig})) {
                        $('#product-button-customer').hide();
                        $('#product-button-customer-tup').hide(); //add by minghua - 20190830 - for remove 'contactme' button
                        $('#product-button-customer-portable').show();
                    } else if(selectedCode !='PBT' && selectedCode !='PBC') {
                        if (selectedCode =='TUP') {
                            $('#product-button-customer').hide();
                            $('#product-button-customer-tup').show(); //add by minghua - 20190830 - for remove 'contactme' button  
                            $('#product-button-customer-portable').hide();
                        }else if(selectedCode =='CBO'){    //Combo Product
                            $('#product-button-customer').hide();
                        }else if(selectedCode =='PPC'){//Rancho_JR1898_20210325
                            $('#product-button-customer').hide();
                            $('#product-button-customer-tup').hide();
                            $('#product-button-customer-portable').hide();
                        }else{
                            $('#product-button-customer').show();
                            $('#product-button-customer-tup').hide(); //add by minghua - 20190830 - for remove 'contactme' button
                            $('#product-button-customer-portable').hide();
                        }
                    } else {
                        $('#product-button-customer').hide();
                        $('#product-button-customer-tup').hide(); //add by minghua - 20190830 - for remove 'contactme' button
                        $('#product-button-customer-portable').hide();
                    }
                    // Modified by IBM(Roy) due to PEI-98 END

                    //Owen_20230103_2023 JOBR-295 for disable 'get a quote' button in custom portal
                    var getCustomQuoteButtonCheck={!getCustomQuoteButtonCheck};
                    if(getCustomQuoteButtonCheck){
                        $('#customerGetQuoteMobile').hide();
                        $('#customerGetQuoteDesktop').hide();
                    }else{
                        $('#customerGetQuoteMobile').show();
                        $('#customerGetQuoteDesktop').show();
                    }
                }
                
                function finishLoading(showElems) {
                    var isProductInfo = false;
                    $('#content-placeholder-area').fadeOut(function(){
                        showElems.forEach(function(elem){
                            $(elem).fadeIn();
                            if ($('#product-filter-wrapper').is(':visible') && elem == '#product-selection-wrapper') {
                                $(elem).fadeOut();                            
                            }
                            if (elem == '#product-info-wrapper') {
                                $('div.header-bar').css('border-bottom', '3px solid #39B54A');
                                isProductInfo = true;
                            }
                        });
                        if (!isProductInfo) {
                            $('div.header-bar').css('border-bottom', 'none');
                        }
                    });
                }
                
                function goBack(evt) {
                    document.getElementById('product-back').style.display="none";
                    if(sessionStorage.getItem('get-protected-back-url')) {
                        var backUrl = sessionStorage.getItem('get-protected-back-url');
                        sessionStorage.removeItem('get-protected-back-url');
                        window.location.href = backUrl;
                    } else if (Cigna.Util.isBrokerPortal()) {
                        Cigna.Util.navigateToVFP("ProductDeckVFP", null, null, true);
                    } else {
                        
                        if ($('#product-info-wrapper').is(':visible')) {
                            $('#product-info-wrapper').fadeOut(function(){
                                $('#product-selection-wrapper').fadeIn();
                                $('div.header-bar').css('border-bottom', 'none');
                                
                                if($('#current-select-Category').val() != '{!JSINHTMLENCODE($CurrentPage.Parameters.Category)}'){
                                    ServerGetProtected.loadProductList($('#current-select-Category').val());
                                }
                                
                            });
                        } else if ($('#product-selection-wrapper').is(':visible')) {
                            
                            $('#product-back').fadeOut();
                            $('#product-selection-wrapper').fadeOut(function(){
                                $('#product-filter-wrapper').fadeIn();
                                $('div.header-bar').css('border-bottom', 'none');
                            });
                           
                        }
                        
                        $('#current-product-code').val('');
                        
                    }
                    evt.preventDefault();
                    if ($('#product-filter-wrapper').is(':visible') && $('#product-selection-wrapper').is(':visible')) {
                        $('#product-selection-wrapper').fadeOut();                    
                    }
                }
                
                function getQuote() {
                    var selectedCode = $('#current-product-code').val();
                    if (Cigna.Util.isBrokerPortal()) {
                        if({!selectedProduct.Product_Code__c == 'LHB'}){
                            Cigna.Util.navigateToVFP("GroupProductQuotationVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);                  
                        }
                        else if(selectedCode == 'PBT'){
                            Cigna.Util.navigateToVFP("PortableGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);                  
                        }
                        else  if(selectedCode == 'PBC'){
                            Cigna.Util.navigateToVFP("PortableConversionGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);                  
                        }else if(selectedCode == 'VHIS'){
                            Cigna.Util.navigateToVFP("GenericGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);   
                        }
                        else{
                            //Cigna.Util.navigateToVFP("CustomerSearchVFP?plan=" + $('#current-product-code').val() + "&Category=" + $('#current-select-Category').val(), null, null, true);
                            Cigna.Util.navigateToVFP("GetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);
                        }
                    } else {
                        // Rancho_HKITSFDC58_20241209 - redirect to website get quote page
                        // if(selectedCode == 'PBT'){
                        //     Cigna.Util.navigateToVFP("PortableGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);                  
                        // }
                        // else  if(selectedCode == 'PBC'){
                        //     Cigna.Util.navigateToVFP("PortableConversionGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);                  
                        // }else if(selectedCode == '{!TopUpProductCode}'){
                        //     Cigna.Util.navigateToVFP("TopUpGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);   
                        // }else if(selectedCode == 'VHIS'){
                        //     Cigna.Util.navigateToVFP("GenericGetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);
                        // }else
                        // {
                        //     Cigna.Util.navigateToVFP("GetQuoteVFP?plan=" + $('#current-product-code').val() + "&Category=" + encodeURIComponent($('#current-select-Category').val()), null, null, true);
                        // }
                    }
                }
                
                function init() {
                    if ("{!selectedProductCode}" != "") {
                        $('#product-filter-wrapper').hide();
                        $('#product-selection-wrapper').hide();
                        Cigna.GetProtected.finishLoading(['#product-info-wrapper', '#action-bar', '#product-back']);

                        if(!Cigna.Util.isBrokerPortal()){
                            var prdname = "{!selectedProduct.Name}";
                            if (prdname!=""){
                                var lang='{!userLanguage}';
                                if (lang.substring(0,2)=='en')
                                    lang = 'en_us';
                                GTMProductClickPush(prdname,lang);
                            }
                        }
                    } else {
                        //JOBR127-Remove Product in Customer Portal-20220524-Kirk
                        //$('#product-filter-wrapper').show();
                        Cigna.GetProtected.selectCategory();
                    }

                    
                    if(Cigna.Util.isBrokerPortal()){
                        $('#product-button-broker').show();
                        $('#product-button-customer').hide();
                        $('#product-button-customer-tup').hide(); //add by minghua - 20190830 - for remove 'contactme' button
                        $('#product-button-customer-portable').hide(); // Added by IBM(Roy) due to PEI-98
                    }else{
                        $('#product-button-broker').hide();
                        //$('#product-button-customer').show();
                        // Modified by IBM(Roy) due to PEI-98
                        if ("{!selectedProductCode}" == "PBC" || "{!selectedProductCode}" == "PBT") {
                            $('#product-button-customer-portable').show();
                            $('#product-button-customer').hide();
                            $('#product-button-customer-tup').hide();//add by minghua - 20190830 - for remove 'contactme' button
                        } else {
                            if("{!selectedProductCode}" == "TUP"){   //add by Nicole- 20190906 - for remove 'contactme' button
                                $('#product-button-customer').hide();
                                $('#product-button-customer-tup').show();//add by minghua - 20190830 - for remove 'contactme' button
                                $('#product-button-customer-portable').hide();
                            }else{
                                $('#product-button-customer').show();
                                $('#product-button-customer-tup').hide();//add by minghua - 20190830 - for remove 'contactme' button
                                $('#product-button-customer-portable').hide();
                            }
                        }
                        // Modified by IBM(Roy) due to PEI-98
                    }
                }
                
                function contactMe(){
                    Cigna.Util.navigateToVFP("ContactMeVFP?plan=" + $('#current-product-code').val(), null, null, true);
                    
                }
                
                function gotoPaperApplication() {
                    Cigna.Util.navigateToVFP("PaperApplicationVFP?plan=" + $('#current-product-code').val(), null, null, true);  
                }

                function gotoIllustrationForm() {
                    Cigna.Util.navigateToVFP("IllustrationFormVFP?plan=" + $('#current-product-code').val(), null, null, true);
                }

                return {
                    selectCategory : selectCategory,
                    selectProduct : selectProduct,
                    finishLoading : finishLoading,
                    goBack : goBack,
                    getQuote : getQuote,
                    init : init,
                    contactMe : contactMe,
                    gotoPaperApplication : gotoPaperApplication,
                    gotoIllustrationForm : gotoIllustrationForm
                }
            }();
            
            $(document).ready(function(){  
                //CR-56_Link_20220214
                $('#product-filter .cigna-list-group-item').on("click",Cigna.GetProtected.selectCategory);
                $('#product-selection .cigna-list-group-item').on("click",Cigna.GetProtected.selectProduct);
                $('#product-back-btn').on("click",Cigna.GetProtected.goBack);
                Cigna.GetProtected.init();
                
                var categoryCodeParam = '{!JSINHTMLENCODE($CurrentPage.Parameters.Category)}';
                if(categoryCodeParam != null){
                    $('#current-select-Category').val(categoryCodeParam);
                }
                
               // For customer portal app
                if (Cigna.Util.checkWebView() && !Cigna.Util.isBrokerPortal()) {
                
                    var categoryCodeParam = '{!JSINHTMLENCODE($CurrentPage.Parameters.Category)}';
                    var productCodeParam = '{!JSINHTMLENCODE($CurrentPage.Parameters.pc)}';
                    
                    if(categoryCodeParam != null && categoryCodeParam != "" && productCodeParam != null && productCodeParam != ""){
                        
                        $('#product-filter-wrapper').hide();
                        $('#product-selection-wrapper').hide();
                        Cigna.GetProtected.finishLoading(['#product-info-wrapper', '#action-bar', '#product-back']);
                              
                        $('#current-select-Category').val(categoryCodeParam);
                      
                        $('#product-selection-wrapper').fadeOut();
                        $('#product-back').fadeOut();
                        $('#content-placeholder-area').fadeIn();
                        
                        $('#current-step').val('product-info');
                        $('#current-product-code').val(productCodeParam);
                        ServerGetProtected.loadProduct(productCodeParam);

                      
                    }
                    
                }

            });
            
        </script>
    </head>
    <apex:define name="additionalLibraries">
         <!--additional css, bootstrap, jquery,js libraries-->
         <apex:stylesheet value="{!URLFOR($Resource.Cigna_HK_Icons, 'Cigna-HK-Icons.css')}"/>
    </apex:define>

    <apex:form >
        <!-- JOBR127-Remove Product in Customer Portal-20220524-Kirk -->
        <apex:actionFunction action="{!loadProductList}" name="loadProductList" 
                             namespace="ServerGetProtected" rerender="product-list"
                             oncomplete="Cigna.GetProtected.finishLoading(['#product-selection-wrapper']);">
            <apex:param name="selectedCategory" assignTo="{!selectedCategory}" value="" />
        </apex:actionFunction>
        <apex:actionFunction action="{!loadProduct}" name="loadProduct" 
                             namespace="ServerGetProtected" rerender="product-info,get-a-quote-button-desktop,get-a-quote-button-mobile,product-button-customer,action-bar-desktop,customer-button-panel"
                             oncomplete="Cigna.GetProtected.finishLoading(['#product-info-wrapper', '#action-bar', '#product-back']);"><!-- Rancho_JOBR324_20240202 - refresh button --><!-- Rancho_HKITSFDC58_20241209 - refresh button -->
            <apex:param name="selectedProductCode" assignTo="{!selectedProductCode}" value="" />
        </apex:actionFunction>
        <input id="current-step" type="hidden" value="" />
        <input id="current-product-code" type="hidden" value="{!selectedProductCode}" />
        
        <div id="product-back" class="bs row" style="display: none;">
            <div class="bs col-xs-12">
                <p><a id="product-back-btn" href="javascript:history.go(-1);" class="bs cigna-fg-orange-medium"><i class="ci-hk-arrow-29" aria-hidden="true"></i> {!$Label.Back}</a></p>
            </div>
        </div>

        <div id="product-filter-wrapper" style="display: none;">
            <div id="product-filter" class="bs row cigna-content-area cigna-fg-gray-dark cigna-div-table">
                <div class="bs col-xs-12 col-sm-3 cigna-div-table-left"><h4><p>{!$Label.eSales_I_want_to}</p></h4></div>
                <div class="bs cigna-list-group col-xs-12 col-sm-9 cigna-div-table-right">
                    <a href="#" class="bs cigna-list-group-item" data-product-category="{!$Label.eSales_Medical_Health}">
                        {!$Label.eSales_Get_coverages_for_medical_expenses}<i class="ci-hk-arrow-31 cigna-fg-green-medium item-right-arrow" aria-hidden="true"></i>
                    </a>
                    <a href="#" class="bs cigna-list-group-item" data-product-category="{!$Label.eSales_Critical_Illness}">
                        {!$Label.eSales_have_financial_assistance}<i class="ci-hk-arrow-31 cigna-fg-green-medium item-right-arrow" aria-hidden="true"></i>
                    </a>
                    <a href="#" class="bs cigna-list-group-item" data-product-category="{!$Label.eSales_Life_Insurance}">
                        {!$Label.eSales_Protect_from_accidents}<i class="ci-hk-arrow-31 cigna-fg-green-medium item-right-arrow" aria-hidden="true"></i>
                    </a>
                    <a href="#" class="bs cigna-list-group-item" data-product-category="{!$Label.eSales_Disability}">
                        {!$Label.eSales_Reduce_financial_burdens}<i class="ci-hk-arrow-31 cigna-fg-green-medium item-right-arrow" aria-hidden="true"></i>
                    </a>                   
                </div>
            </div>
        </div>
        
        <div id="product-selection-wrapper" style="display:none;">
            <div id="product-selection" class="bs row cigna-content-area cigna-fg-gray-dark cigna-div-table">
                <div class="bs col-xs-12 col-sm-3 cigna-div-table-left"><h4>{!$Label.eSales_I_want_to} <span id="cigna-selected-category"></span></h4></div>
                <apex:outputPanel id="product-list" layout="block" styleClass="bs cigna-list-group col-xs-12 col-sm-9 cigna-div-table-right">
                    <apex:repeat value="{!productList}" var="product">
                        <!--Rancho_Combo_20201204-->
                        <div style='display:none;'>
                            <apex:outputPanel rendered="{!isCustomerPortal}">
                                <a id="website_link" href="{!comboLink}" target="_blank"></a>
                            </apex:outputPanel>
                        </div>
                        <!--IBM JING PORTABLE CR 0420 START-->
                        <apex:outputPanel rendered="{!(product.Product_Code__c != 'PBT' && product.Product_Code__c != 'PBC' && product.Product_Code__c != 'TUP')}">
                              <div class="bs row cigna-list-group-item" data-product-code="{!product.Product_Code__c}" data-product-name="{!product.Name}">
                            <div class="bs col-xs-1 cigna-fg-green-medium"><i class="fa fa-hospital-o fa-2x" aria-hidden="true"></i></div>
                            <div class="bs col-xs-10 cigna-fg-green-medium">
                                <h4 style="text-transform: uppercase;">{!product.Name__c}</h4><br/>
                            </div>
                            <!-- Rancho_JR1898_20210325 -->
                            <apex:outputPanel rendered="{!product.Product_Code__c != 'PPC'}">
                                <div class="bs col-xs-12" style="padding-top: 15px;"><p>{!product.Short_Description__c}</p></div>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!product.Product_Code__c == 'PPC'}">
                                <div class="bs col-xs-12" style="padding-top: 15px;"><p>{!$Label.PPC_Product_List_Description}</p></div>
                            </apex:outputPanel>
                            <div class="bs pull-right item-right-arrow cigna-fg-green-medium"><i class="ci-hk-arrow-31" aria-hidden="true"></i></div>
                        </div>
                        <br/>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!((product.Product_Code__c == 'PBT' && isTopUpElig) || (product.Product_Code__c == 'PBC' && isCoversionElig))}">
                              <div class="bs row cigna-list-group-item" data-product-code="{!product.Product_Code__c}" data-product-name="{!product.Name}">
                            <div class="bs col-xs-1 cigna-fg-green-medium"><i class="fa fa-hospital-o fa-2x" aria-hidden="true"></i></div>
                            <div class="bs col-xs-10 cigna-fg-green-medium">
                                <h4 style="text-transform: uppercase;">{!product.Name__c}</h4><br/>
                            </div>
                            <div class="bs col-xs-12" style="padding-top: 15px;"><p>{!product.Short_Description__c}</p></div>
                            <div class="bs pull-right item-right-arrow cigna-fg-green-medium"><i class="ci-hk-arrow-31" aria-hidden="true"></i></div>
                        </div>
                        <br/>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!(product.Product_Code__c == 'TUP' && IsTopUpEligible)}">
                              <div class="bs row cigna-list-group-item" data-product-code="{!product.Product_Code__c}" data-product-name="{!product.Name}">
