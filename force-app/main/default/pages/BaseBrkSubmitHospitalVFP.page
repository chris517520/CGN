                    var pageCountLabel = "{!$Label.Page_0_of_1}";
                    var rowsPerPage = 20;
                    var currentPage = 1;
                    var totalPage = Math.ceil( $(".pioptions").length / rowsPerPage);;
                    
                    if( $(".pioptions").length > rowsPerPage ){
                        displayPage(1);
                        var pagination = pageCountLabel.replace('{0}',currentPage).replace('{1}',totalPage);
                        $("#pageCount").text(pagination);
                        $("#paginationDiv").show();
                    }
                    
                    
                    function clickPrev(){
                        displayPage(currentPage - 1);
                    }
                    
                    function clickNext(){
                        displayPage(currentPage + 1);
                    }
                    
                    function displayPage(pageNumber){
                        $(".pioptions, #prevPageBtn, #nextPageBtn").hide();
                        var rowCount = 1;
                        var startRow = (pageNumber-1)*rowsPerPage;
                        var endRow = pageNumber*rowsPerPage;
                        $(".pioptions").each(function(){
                            if( rowCount >= startRow && rowCount <= endRow){
                                $(this).show();
                            }
                            rowCount++;
                        });
                        if(pageNumber > 1){
                            $("#prevPageBtn").show();
                        }
                        if(pageNumber < totalPage){
                            $("#nextPageBtn").show();
                        }
                        currentPage = pageNumber;
                        var pagination = pageCountLabel.replace('{0}',currentPage).replace('{1}',totalPage);
                        $("#pageCount").text(pagination);
                    }
                    
                    //CR-56_Link_20220214
                    $('.selectOptions').on("click",function(){
                        $(this).siblings('.selectOptions').removeClass('cigna-fg-orange-dark').addClass('cigna-fg-gray-dark');
