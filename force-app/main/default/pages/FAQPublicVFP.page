        function updateLanguage() {

            var currentURL = document.location.protocol + "//" + document.location.host + document.location.pathname;
            var currentLanguage = window.location.href;
            
            if(currentLanguage.indexOf("en") != -1){
                document.location = currentURL + '?lang=zh_TW';
            }
            else{
                document.location = currentURL + '?lang=en_US';
            }
            
        };
        
        if(Cigna.Util.checkWebView()){
       
           if(Cigna.Util.isIOS()){
       
            document.getElementById("navtopbar").style.padding = "20px 0px 0px 0px";
            document.getElementsByTagName("body")[0].style.padding = "70px 0px 0px 0px";
           
           }
           
        }
        if (Cigna.Util.isIOS() && Cigna.Util.isCustomerPortal() && $(window).width() < 768 ) {
                
                $('html').css('overflow', 'auto');
                $('body').css('overflow-x', 'hidden');
