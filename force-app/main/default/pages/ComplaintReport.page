<apex:page controller="ComplaintReport">
    <apex:pageBlock title="ComplaintReport">
        <apex:pageBlockTable value="{!ComplaintReport}" var="vt">
            <apex:column value="{!vt.row}"/>
            <apex:column value="{!vt.column}"/>
            <apex:column value="{!vt.countData}"/>
            <apex:column value="{!vt.reportCompletionDate}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>