            <apex:actionFunction name="getEncryptedString" action="{!getEncryptedString}" reRender="encryptedSearchCriteria" oncomplete="redirectToClaimDetail(isPassToHereGlobal,claimIdGlobal,policyTypeGlobal);"></apex:actionFunction>
            <apex:actionFunction name="getEncryptedString2" action="{!getEncryptedString}" reRender="encryptedSearchCriteria" oncomplete="redirectToMyClaimSubmit();"></apex:actionFunction>
            <apex:actionFunction name="getEncryptedString3" action="{!getEncryptedString}" reRender="encryptedSearchCriteria" oncomplete="Cigna.Util.navigateToVFP('SubmitHospitalVFP{!IF(encryptedSearchCriteria!='','?'+URLParamName+'='+encryptedSearchCriteria,'')}');"></apex:actionFunction>
