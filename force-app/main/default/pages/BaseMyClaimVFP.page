            function redirectToEditClaim(recordType, cId) {
                if (recordType == 'Pre Admission') {
                    Cigna.Util.navigateToVFP('SubmitHospitalVFP?claimId=' + cId, null, null, true);
                } else {
                    Cigna.Util.navigateToVFP('MyClaimSubmitVFP?cId=' + cId, null, null, true);
                }
            }

            //JR1840 jack add
            function redirectToClaimDetail(isPassToHere, claimId, policyType) {
                console.log('isPassToHere '+ isPassToHere +' claimId '+claimId+' policyType '+policyType);
                console.log($('[id$="encryptedSearchCriteria"]').val());
                var encryptedSearchCriteria = $('[id$="encryptedSearchCriteria"]').val();
                var encryptedSearchCriteria2 = '';
                if(encryptedSearchCriteria){
                    encryptedSearchCriteria2 = '&'+'{!URLParamName}'+'='+ encryptedSearchCriteria;
                    encryptedSearchCriteria = '?'+'{!URLParamName}'+'='+ encryptedSearchCriteria;
                }else{
                    encryptedSearchCriteria ='';
                }
                console.log(encryptedSearchCriteria);
                if (isPassToHere) {
                    if (policyType == 'Group') {
                        sessionStorage.setItem('policy-detail-back-url', 'MyClaimVFP'+ encryptedSearchCriteria);
                        Cigna.Util.navigateToVFP('ClaimDetailsVFP?claimNo=' + claimId + '&policyType=' + policyType , null, null, true);
                    } else {
                        sessionStorage.setItem('policy-detail-back-url', 'MyClaimVFP'+ encryptedSearchCriteria);
                        Cigna.Util.navigateToVFP('ClaimDetailsVFP?claimNo=' + claimId , null, null, true);
                    }
                }
            }
            var isPassToHereGlobal;
            var claimIdGlobal;
            var policyTypeGlobal;
            function getEncryptedCriteria(isPassToHere, claimId, policyType){
                isPassToHereGlobal = isPassToHere;
                claimIdGlobal = claimId;
                policyTypeGlobal = policyType;
                getEncryptedString();

            }

            function getEncryptedCriteriaMakeClaim(){
                
                getEncryptedString2();

