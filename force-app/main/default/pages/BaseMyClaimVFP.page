            <apex:actionFunction status="loader-icon" action="{!checkIPMIPolicy}" name="checkIPMIPolicy" oncomplete="loadDataAtAction();" reRender="desktopBtn, mobileBtn, brkMobileBtn"/> <!--Updated by Nicole-->
