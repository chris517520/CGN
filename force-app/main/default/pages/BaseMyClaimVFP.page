            function greySeparatorStyle() {
                /**grey separator**/
                var srlist = document.getElementsByClassName("listcontent");
                var flip = 0;
                for (var i = 0; i < srlist.length; i++) {
                    var item = srlist[i];
                    if (flip == 0) {
                        item.style.backgroundColor = '#f2f2f2';
                        flip = 1;
                    } else if ($(item).is(':first-child')) {
                        item.style.backgroundColor = '#f2f2f2';
                    } else {
                        flip = 0;
                    }
                }
            }

            function selectedTab(item, idx) {

                $('#inputCharErrorMessage').css('display','none');
                $('#tooManyError').hide();
                

                if (idx == 1) {
                    $('#tab_in_progress').removeClass('active-bar-tab').addClass('inactive-bar-tab');
                    $('#tab_completed').removeClass('active-bar-tab').addClass('inactive-bar-tab');

                    $('#claim_in_progress').hide();
                    $('#claim_completed').hide();

                    $('#claim_all').show();
                    $('#tab_all').addClass('active-bar-tab');

                    //Rancho_JR1878_20210104
                    $('#policy_in_progress').hide();
                    $('#policy_completed').hide();
                    $('#policy_all').show();
                } else if (idx == 2) {
                    console.log('rancho_idx000-->'+idx);
                    $('#tab_all').removeClass('active-bar-tab').addClass('inactive-bar-tab');
                    $('#tab_completed').removeClass('active-bar-tab').addClass('inactive-bar-tab');

                    $('#claim_all').hide();
                    $('#claim_completed').hide();

                    $('#claim_in_progress').show();
                    $('#tab_in_progress').addClass('active-bar-tab');

                    //Rancho_JR1878_20210104
                    $('#policy_all').hide();
                    $('#policy_completed').hide();
                    $('#policy_in_progress').show();
                } else if (idx == 3) {
                    $('#tab_all').removeClass('active-bar-tab').addClass('inactive-bar-tab');
                    $('#tab_in_progress').removeClass('active-bar-tab').addClass('inactive-bar-tab');

                    $('#claim_all').hide();
                    $('#claim_in_progress').hide();

                    $('#claim_completed').show();
                    $('#tab_completed').addClass('active-bar-tab');

                    //Rancho_JR1878_20210104
                    $('#policy_all').hide();
                    $('#policy_in_progress').hide();
                    $('#policy_completed').show();
                }

                var selectedType = $("[id$='selectedSearchOptionValue']").val();
                var selectedValue = $("[id$='selectedSearchValueGroup']").val();

                if(Cigna.Util.isBrokerPortal()){
                    var opt = $("[id$='selectedSearchOptionValue']").find(":selected").val();
                    var searchvalue = $("input[id*='selectedSearchValueGroup']").val().split(' ').join('');
                    //Rancho_JR1878_20210104
                    if(opt == 'Person_Insured' || opt == 'Policy_Holder_Name' && searchvalue.length!= 0){
                        //var searchvalue = document.querySelectorAll('input[id^="selectedSearchValue"]').value;
                        //var searchvalue = $('#claimListSearchValue').text(); replace(/\s+/g, '').

                        if((searchvalue.length) < 2){
                            $('#inputCharErrorMessage').text('{!$Label.Please_input_at_least_2_characters_to_search}');
                            $('#inputCharErrorMessage').css('display','block');
                        }
                    }
                    
                brkInitPagingAtAction(idx,selectedType,selectedValue);

                } else {
                    console.log('customer portal');
                    cusInitPagingAtAction(idx);
                }
                // to disable initial search
                //refreshTabWithoutInitialSearch(idx);
            }

            function clickPrev() {
                handlePrevPageAtAction();
            }

            function clickNext() {
                console.log('next');
                handleNextPageAtAction();
            }
            var delType = '';
            var idx = '';
            var idx_progress = '';
            var idx_completed = '';

            function delChange(list, type, rowNum) {
                delType = list;
                if (type != null) {
                    if (type == 'all' && rowNum != null) idx = rowNum;
                    if (type == 'progress' && rowNum != null) idx_progress = rowNum;
                    if (type == 'completed' && rowNum != null) idx_completed = rowNum;
                }

                var overlay = document.getElementById('overlay');
                var delConfirmBox = document.getElementById('delConfirmBox');
                overlay.style.opacity = .95;
                if (overlay.style.display == "block") {
                    overlay.style.display = "none";
                    delConfirmBox.style.display = "none";
                } else {
                    overlay.style.display = "block";
                    delConfirmBox.style.display = "block";
                }
            }

            function delChange2(list, type, rowNum) {
                delType = list;
                if (type != null) {
                    if (type == 'all' && rowNum != null) idx = rowNum;
                    if (type == 'progress' && rowNum != null) idx_progress = rowNum;
                    if (type == 'completed' && rowNum != null) idx_completed = rowNum;
                }

                var overlay = document.getElementById('overlay2');
                var delConfirmBox = document.getElementById('delConfirmBox2');
                overlay.style.opacity = .95;
                if (overlay.style.display == "block") {
                    overlay.style.display = "none";
                    delConfirmBox.style.display = "none";
                } else {
                    overlay.style.display = "block";
                    delConfirmBox.style.display = "block";
                }
            }

            function delChange3(list, type, rowNum) {
                delType = list;
                if (type != null) {
                    if (type == 'all' && rowNum != null) idx = rowNum;
                    if (type == 'progress' && rowNum != null) idx_progress = rowNum;
                    if (type == 'completed' && rowNum != null) idx_completed = rowNum;
                }

                var overlay = document.getElementById('overlay3');
                var delConfirmBox = document.getElementById('delConfirmBox3');
                overlay.style.opacity = .95;
                if (overlay.style.display == "block") {
                    overlay.style.display = "none";
                    delConfirmBox.style.display = "none";
                } else {
                    overlay.style.display = "block";
                    delConfirmBox.style.display = "block";
                }
            }

            function confirmDel(choice) {
                if (choice == 'true') {
                    //pass idx to delete at controller
                    deleteSingleClaimRecordAllAtAction(idx);
                }
                document.getElementById('overlay').style.display = "none";
                document.getElementById('delConfirmBox').style.display = "none";
            }

            function confirmDelInProgress(choice) {
                if (choice == 'true') {
                    //pass idx to delete at controller
                    deleteSingleClaimRecordProgressAtAction(idx_progress);
                }
                document.getElementById('overlay2').style.display = "none";
                document.getElementById('delConfirmBox2').style.display = "none";
            }

            function confirmDelCompleted(choice) {
                if (choice == 'true') {
                    //pass idx to delete at controller
                    deleteSingleClaimRecordCompletedAtAction(idx_completed);
                }
                document.getElementById('overlay3').style.display = "none";
                document.getElementById('delConfirmBox3').style.display = "none";
            }


